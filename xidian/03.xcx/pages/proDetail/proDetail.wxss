.spec_capacity button::after {
  border: none;
}

.spec_capacity .button_click {
  color: #0d8d37;
  border: 2rpx solid #0d8d37;
}

.spec_num text {
  color: #9d9d9d;
  font-size: 24rpx;
  margin-bottom: 20rpx;
}

.spec_num .pro_icon {
  margin-right: 4rpx;
