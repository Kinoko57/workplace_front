/* pages/myCollection/myCollection.wxss */

page {
  background: #f6f6f6;
}

.my_collection {
  font-family: SimHei;
  background: #f6f6f6;
  overflow-x: hidden;
}

.coll_header {
  text-align: center;
  /* background: #ebebeb; */
  height: 86rpx;
  line-height: 86rpx;
  display: flex;
  justify-content: space-between;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 100;
  background: #f6f6f6;
}

.coll_header_null {
  height: 86rpx;
}

.coll_header text {
  padding-left: 26rpx;
  color: #515151;
  font-size: 24rpx;
}

.coll_header .coll_num {
  color: #e13816;
  font-size: 26rpx;
  padding-left: 0;
}

.coll_header .right {
  display: inline-flex;
  align-items: center;
}

.coll_header .right .coll_edit {
  padding-right: 26rpx;
  color: #e13816;
  font-size: 24rpx;
}

.coll_header .right .line {
  width: 2rpx;
  height: 30rpx;
  border-right: 2rpx solid #888;
}

.coll_header .right .iconfont {
  padding: 0 26rpx;
  font-size: 36rpx;
}

.coll_body {
  height: 100%;
}

.coll_body .editDel {
  height: 100rpx;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: fixed;
  bottom: 0;
  width: 100%;
  z-index: 50;
box-shadow:0px 0px 24rpx 0px rgba(0, 0, 0, 0.19);
}

.editDel_null {
  height: 100rpx;
}

.editDel_null_iphoneX {
  height: 168rpx;
}

.coll_body .editDel_iphonex {
  padding-bottom: 68rpx;
}

.editDel .allCheck {
  margin-left: 26rpx;
  font-size: 26rpx;
  color: #555;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.editDel .coll_btn {
  width: 240rpx;
}

.editDel .coll_btn button {
  width: 156rpx;
  margin: 0 auto;
  padding: 0;
  border-radius: 0;
  color: #fff;
  font-size: 26rpx;
  font-weight: normal;
  height:60rpx;
  line-height:60rpx;
  border:1px solid rgba(255,0,0,1);
  color: rgba(255,0,0,1);
  border-radius: 50rpx;
}

.editDel .coll_btn .disbale_btn {
  background: rgba(225, 56, 22, 0.5);
}

.coll_list {
}

/* 订单商品信息 */

.coll_list .coll_pro {
  /*  */
  height: 296rpx;
  background: #fff;
  margin-bottom: 20rpx;
  display: flex;
  align-items: center;
  flex-direction: row;
  overflow: hidden;
  margin: 20rpx;
  border-radius: 12rpx;
  /* border-bottom: 1px solid #ebebeb; */
}
.coll_list .coll_pro:first-child{
  margin-top: 0;
}
.coll_list .coll_pro movable-area {
  width: 750rpx;
  height: 296rpx;
}

.coll_list .coll_pro movable-area movable-view {
  width: 750rpx;
  height: 296rpx;
}

.coll_list .coll_pro movable-area movable-view .main_item_content {
  box-sizing: border-box;
  background: #ffff;
  width: 750rpx;
  height: 296rpx;
}

.coll_list .coll_pro .pro_block {
  display: inline-flex;
  align-items: center;
  height: 296rpx;
  /* margin: 0 26rpx; */
  background: #fff;
  width: 700rpx;
}

.coll_list .coll_pro .pro_block_short {
  width: 638rpx;
}

.coll_pro .pro_chose {
  float: left;
  display: flex;
  align-items: center;
  height: 296rpx;
  margin-left: 12rpx;
}

.coll_pro .pro_img {
  float: left;
  /* margin-top: 40rpx; */
  margin-left: 18rpx;
  margin-right: 30rpx;
  position: relative;
}

.pro_img image {
  width: 220rpx;
  height: 220rpx;
}

.pro_img .pro_tips {
  width: 220rpx;
  height: 52rpx;
  background: rgba(0, 0, 0, 0.4);
  line-height: 52rpx;
  text-align: center;
  position: absolute;
  bottom: 0;
}

.pro_tips text {
  color: #fff;
  font-size: 24rpx;
}

.coll_pro .pro_info {
  float: left;
  width: 420rpx;
}

.coll_pro .pro_info_edit {
  width: 360rpx;
}

.pro_info .pro_title {
  /* width: 440rpx; */
  font-size: 26rpx;
  color: #333;
  /* margin-top: 50rpx; */
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  line-height: 1.6;
}
.pro_info .pro_desc{
  margin: 0 auto;
  font-size:24rpx;
  color:rgba(153,153,153,1);
  overflow: hidden;
  /*超出部分隐藏*/
  white-space: nowrap;
  /*不换行*/
  text-overflow: ellipsis;
  margin-bottom: 10rpx;
}
.pro_title image {
  width: 68rpx;
  height: 24rpx;
}

.pro_title  .pro_icon {
  background: #e13816;
  height: 26rpx;
  padding: 0 6rpx;
  font-size: 20rpx;
  color: #fff;
  border-radius: 8rpx;
  line-height: 26rpx;
  text-align: center;
  display: inline-block;
  margin-right: 10rpx;
}

.pro_title  .pro_level {
  width: 68rpx;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  position: relative;
  height: 24rpx;
}

.pro_title  .pro_level text {
  font-size: 16rpx;
  color: #fefefe;
  position: absolute;
  left: 46%;
  top: 50%;
  transform: translate(-50%, -50%);
  white-space: nowrap;
}

.pro_info .pro_spec {
  width: 100%;
  /* width: 440rpx; */
  overflow: hidden;
  margin-top: 20rpx;
  height: 64rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.pro_spec .pro_price {
  font-family: 'DIN';
  width: 280rpx;
  display: flex;
  align-items: center;
}

.pro_price .pro_nowprice {
  color: #e13816;
  font-size: 30rpx;
  font-weight: bold;
}

.pro_price .pro_integral {
  font-size: 30rpx;
  font-weight: bold;
  color: #e13816;
  /* font-weight: bold; */
}

.pro_price .pro_inter_img {
  display: flex;
  align-items: flex-end;
  /* margin-right: 6rpx; */
}

.pro_price .pro_inter_img .iconfont {
  font-size: 26rpx;
  display: inline-flex;
  color: #e13816;
  font-weight: bold;
}

.pro_price .pro_orig {
  font-size: 22rpx;
  color: #7c7c7c;
  text-decoration: line-through;
}

.pro_spec .pro_icon {
  margin-right: 4rpx;
  display: flex;
}

.pro_icon image {
  width: 50rpx;
  height: 46rpx;
}

.pro_icon button {
  width: 170rpx;
  height: 52rpx;
  background: #e13816;
  color: #fff;
  font-size: 26rpx;
  padding: 0rpx 20rpx;
  line-height: 52rpx;
  border-radius: 50rpx;
}

.coll_list .delete_btn {
  width: 184rpx;
  height: 296rpx;
  background-color: #e86d56;
  border-top-right-radius: 10px;
  border-bottom-right-radius: 10px;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
  line-height: 296rpx;
}

.order-foot {
  height: 84rpx;
  line-height: 84rpx;
  /* background: #fff; */
  text-align: center;
  margin-top: -20rpx;
}

.order-foot text {
  font-size: 23.97rpx;
  color: #bebebe;
}

.coll_list_null {
  text-align: center;
  font-size: 30rpx;
  /* font-weight: bold; */
  padding: 200rpx 0;
  color: #999;
}

.coll_list_null image {
  display: inline-block;
  width: 750rpx;
  height: 357rpx;
}

/* 空白的块，解决底部导航占领高度 */

.null_height {
  height: 168rpx;
}

/* 收藏列表样式2 */

.coll_list_block {
  /* overflow: hidden; *//* height: 716rpx; */
  margin-top: 20rpx;
}

.coll_block {
  position: relative;
  float: left;
  /* margin: 13rpx; */
  text-align: center;
  width: 240rpx;
  margin-right: 14rpx;
  margin-bottom: 14rpx;
  position: realtive;
}

.coll_list_block .coll_block:nth-child(3n+3) {
  margin-right: 0;
}

.coll_block .block_img {
  width: 240rpx;
  height: 240rpx;
}
.coll_block .pro_tips{
  position: absolute;
  top: 186rpx;
  width: 100%;
  width: 240rpx;
  background: rgba(0, 0, 0, 0.4);
  line-height: 50rpx;
  text-align: center;
}
.coll_block checkbox {
  position: absolute;
  right: -2rpx;
  top: 0;
}

.coll_block .pro_price {
  background: #fff;
  display: inline-flex;
  padding: 16rpx 10rpx;
  justify-content: space-between;
  align-items: center;
  width: 220rpx;
  font-size: 26rpx;
  color: #e13816;
  font-family: 'DIN';
}

.coll_block .pro_price text {
  display: inline-flex;
  align-items: center;
}

.coll_block .pro_price .iconfont {
  color: #515151;
  display: inline-flex;
  align-items: center;
  font-size: 32rpx;
}
