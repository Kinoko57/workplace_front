page{
  background: #f6f6f6;
}
.after_detail_body{

}
.after_detail_pro{
  background: #fff;
  color: #666;
  font-size: 18rpx;
  text-align: center;
  padding: 20rpx 0 10rpx;
}
.after_detail_top{
  color: #fff;
  padding: 0 40rpx 60rpx;
}
.after_detail_status{
  padding: 30rpx 0 20rpx;
  font-size: 34rpx;
}
/* 进度 */
.after_datail_progress{

}

.vip_sign_date_wraper{
  display: flex;
}
.vip_sign_block .vip_sign_t{
   display: flex;
   align-items: center;
}
.vip_sign_block .vip_sign_t icon{
  display: inline-flex;
  align-items: center;
  font-size: 28rpx;
}
.progress_line{
  width: 100rpx;
  height: 4rpx;
  background: #fff;
}
.vip_sign_block:last-child .progress_line{
  width: 0;
}
.vip_sign_block .vip_sign_b{
   font-size: 20rpx;
   color: #fff;
   padding-top: 10rpx;
}
.after_detail_bot{
  padding: 0 20rpx;
  position: relative;
  top: -30rpx;
}
.after_detail_block{
  background: #fff;
  border-radius: 20rpx;
  padding:30rpx;
  margin-bottom:30rpx ;
}
.server_msg{
  font-size: 22rpx;
  color: #333;
}
.server_msg text:nth-child(2){
  font-size: 22rpx;
  color: #999;
  display: block;
  padding-top: 15rpx;
}
.pro_block{
  padding-bottom: 20rpx;
}
.pro_block .pro_block_titi{
  font-size: 30rpx;
  color: #333;
  padding-bottom: 20rpx;
}
.pro_block .pro_block_list{
   display: flex;
}
.pro_block_list .pro_block_list_img{
  width: 134rpx;
  height: 134rpx;
}
.pro_block_list .pro_block_list_info{
   width: 520rpx;
   padding-left: 20rpx;
   color: #999;
   font-size: 24rpx;
   padding-top: 10rpx;
}
.pro_block_list_info .pro_block_list_titi{
  line-height: 1.4;
  display: -webkit-box;
  color: #666;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  word-break: break-all;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  width: 460rpx;
}
.pro_block_list_info .pro_block_list_pri{
  display: flex;
  font-size: 22rpx;
  padding-top: 14rpx;
}
.pro_block_list_pri .list_pri:nth-child(2){
   padding-left: 20rpx;
}
.pro_block_list_pri .list_pri .pri{
  color: #333;
  font-size:24rpx ;
}
.server_order{
  font-size: 24rpx;
  border-bottom: 1px solid #ededed;
  padding-bottom: 25rpx;
  padding-top: 15rpx;
}
.server_order:last-child{
  border-bottom: 0;
}
.server_order:first-child{
  padding-top: 0;
}
.server_order_block{
  display: flex;
  padding: 20rpx 0;
}
.server_order_block_l{
   color: #999;
}
.server_order_block_r{
   color: #333;
   width: 500rpx;
   padding-left: 6rpx;
   word-break: break-all;
}
.child_block{
  /* display: flex;
  align-items: center;
  justify-content: space-between; */
  margin-bottom: 20rpx;
}
.child_block .block_left{
  display: inline-flex;
  align-items: center;
}
.block_left .add_info_l_ku{
  width: 50rpx;
  height:50rpx;
  border-radius: 50%;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  margin-right: 8rpx;

}
.block_left .add_info_l_ku_sex{
  border:1px solid #f56c6c;
  color: #f56c6c;
}
.block_left .add_info_l_ku_man{
  border:1px solid #409eff;
  color: #409eff;
}
.child_block .block_left .people_name{
  width: 300rpx;
  text-align: left;
  margin-left: 6px;
  white-space:nowrap;/*规定段落中的文本不进行换行*/
  overflow:hidden;/*内容会被修剪，并且其余内容是不可见的。*/
  text-overflow:ellipsis;/*显示省略号来代表被修剪的文本*/
}

.server_order_block_copy{
  display: inline-block;
  height:32rpx;
  line-height: 32rpx;
  font-size: 18rpx;
  width: 70rpx;
  text-align: center;
  color: #0d8d37;
  border:1px solid #0d8d37;
  border-radius: 16rpx;
  margin-left: 10rpx;
}
.after_server{
  display: flex;
  align-items: center;
  justify-content: center;
}
.server_delet{
  color: #999;
  border-radius: 18rpx;
  height: 48rpx;
  line-height: 48rpx;
  font-size: 22rpx;
  border:1px solid #999;
  width: 180rpx;
  text-align: center;
  margin: 0 20rpx;
}
.server_fixed{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 710rpx;
  background: #fff;
  text-align: right;
  padding: 20rpx;
}
.server_fixed text{
  display: inline-block;
  width: 136rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius: 30rpx;
}

