/* pages/orderDetail/orderDetail.wxss */
page{
  background: #f6f6f6;
}
.pub_lcolor{
  font-size: 24rpx;
  max-width: 530rpx;
  word-break: break-all;
}
.pub_rcolor{
  font-size: 24rpx;
  color: #666;
  display: flex;
  align-items: center;
}
.pub_rcolor icon{
  display: inline-flex;
  font-size: 23rpx;
  align-items: center;
}
.or_top{
  color: #fff;
  padding: 30rpx 20rpx 35rpx;
  font-size: 34rpx;
  display: flex;
  align-items: center;
}
.or_top icon{
  display: inline-flex;
  align-items: center;
  font-size: 40rpx;
}
.or_top_status{
  padding-left: 20rpx;
  font-size: 34rpx;
}
.or_top_pay{
  padding-left: 24rpx;
  font-size: 28rpx;
  padding-top: 20rpx;
  word-break: break-all;
}
.countdown_bg{
  background: #fff;
  width: 100%;
  padding: 20rpx 0;
  border-bottom: 1px solid #ededed;
}
.countdown_bg .countdown_block{
  font-size: 24rpx;
  padding:0 30rpx;
  display: flex;
  align-items: center;
}
.countdown_bg .countdown_block text{
  margin: 0 4rpx;
}
.or_wra{
  margin-bottom: 40rpx;
}
.pay_other_list{
  margin-top: 20rpx;
  background: #fff;
  padding: 44rpx 20rpx;
}
.pay_other_list_flex{
   display: flex;
   align-items: center;
}
.supplier_style{
  padding-bottom: 10rpx;
}
.pay_other_list image{
  width: 250rpx;
  height: 250rpx;
  border-radius: 8rpx;
  margin-right: 22rpx;
}
.pay_other_tit{
 font-size: 26rpx;
 color: #333;
 padding-top: 8rpx;
 width: 430rpx;
 overflow: hidden;
 text-overflow: ellipsis;
 white-space: nowrap;
}
.pay_other_spec{
 font-size: 24rpx;
 color: #999;
 padding-top: 20rpx;
 width: 430rpx;
 overflow: hidden;
 text-overflow: ellipsis;
 white-space: nowrap;
}
.pay_other_mun{
  font-size: 22rpx;
 color: #999;
}
.pay_other_pri{
 font-size: 28rpx;
 display: flex;
 align-items: center;
 justify-content: space-between;
 padding-top: 26rpx;
}
.or_status{
  background: #fff;
  padding: 30rpx 20rpx;
  margin-top: 20rpx;
  color: #666;
  font-size: 24rpx;
}
.or_height{
  line-height: 50rpx;
}
.pay_disc{
  display: flex;
  font-size: 24rpx;
  align-items: center;
  justify-content: space-between;
  line-height: 60rpx;
}
.pay_discount{
  border-bottom: 1px solid #ededed;
  padding:0 0 20rpx;
}
.pay_bottom{
  /* border-top: 1px solid #ededed; */
  padding-bottom: 0;
  border-bottom: none;
}
/* 我的学员列表排版 */
.or_status  .pay_child_title{
  padding-bottom: 20rpx;
  border-bottom: 1px solid #ededed;
}
.or_status .pay_child_list{
  margin-top: 20rpx;
}
.or_status .pay_child_list .child_block{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.or_status .pay_child_list .child_block .block_left{
  display: inline-flex;
  align-items: center;
}
.or_status .pay_child_list .child_block .block_left .add_info_l_ku{
  width: 60rpx;
  height:60rpx;
  border-radius: 50%;
  font-size: 28rpx;
  margin:0 auto;
  display: inline-flex;
  align-items: center;
  justify-content: center;
}
.or_status .pay_child_list .child_block .block_left .add_info_l_ku_sex{
  border:1px solid #f56c6c;
  color: #f56c6c;
}
.or_status .pay_child_list .child_block .block_left .add_info_l_ku_man{
  border:1px solid #409eff;
  color: #409eff;
}
.or_status .pay_child_list .child_block .block_left .people_name{
  width: 300rpx;
  text-align: left;
  margin-left: 20rpx;
  white-space:nowrap;/*规定段落中的文本不进行换行*/
  overflow:hidden;/*内容会被修剪，并且其余内容是不可见的。*/
  text-overflow:ellipsis;/*显示省略号来代表被修剪的文本*/
}
.or_status .pay_child_list .child_block .block_left .iconfont{
  font-size: 40rpx;
}
.or_status .pay_child_list .child_block .block_right{
  display: inline-flex;
  align-items: center;
  font-size: 24rpx;
}
.or_status .pay_child_list .child_block .block_right text{
  display: inline-flex;
  align-items: center;
}
.or_status .pay_child_list .child_block .block_right .iconfont{
  display: inline-flex;
  align-items: center;
}
.copy{
  display: inline-flex;
  padding:0 10rpx;
  height: 34rpx;
  align-items: center;
  font-size: 20rpx;
  border:1px solid #0d8d37;
  border-radius: 10rpx;
  margin-left: 15rpx;
}
.or_fixed{
  position: fixed;
  bottom: 0;
  width: 100%;
  background: #fff;
  border-top: 1px solid #ededed;
}
.or_fixed_flex{
  padding: 30rpx 30rpx;
  text-align: right;
}
.or_fixed .or_fixed_flex .button{
  display: inline-flex;
  font-size: 22rpx;
  height: 60rpx;
  padding: 0;
  justify-content: center;
  align-items: center;
  width: 160rpx;
  border-radius: 25rpx;
  background: none;
  margin-left: 15rpx;
}
button.cancel_text{
  background-color:#fff;
}
.or_fixed .or_fixed_flex .btn_color{
  display: inline-flex;
  font-size: 22rpx;
  height: 60rpx;
  padding: 0;
  justify-content: center;
  align-items: center;
  width: 160rpx;
  border-radius: 25rpx;
  /* background: none; */
  margin-left: 15rpx;
}
.active_text{
  color: #fff;
  /* border: 1px solid #0d8d37; */
}
.cancel_text{
  border: 1px solid #666;
  color: #333;
}
/* 团购 */
.pro_type{
  display: inline-flex;
  height: 28rpx;
  width: 60rpx;
  font-size: 18rpx;
  justify-content: center;
  align-items: center;
  border:1px solid #0d8d37;
  border-radius: 19rpx;
  margin-top: -3rpx;
  vertical-align: middle;
}
/* 支付宝弹框 */
.alipay_dia{
  position: fixed;
  width:66%;
  z-index: 1001;
  padding-top: 20rpx;
  background:#fff;
  color: #333;
  font-size: 26rpx;
  top: 50%;
  left: 50%;
  border-radius:20rpx ;
  transform:translate(-50%,-50%);
}
.alipay_dia .alipay_text{
  padding: 30rpx 20rpx 50rpx;
  text-align: center;
}
.alipay_dia .alipay_btn{
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 20rpx 0 30rpx;
}
.alipay_dia .alipay_btn text{
  display: inline-block;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 25rpx;
  padding: 0 40rpx;
  margin: 0 20rpx;
}
.alipay_dia .alipay_btn text:nth-child(1){
  border: 1px solid #999;
}
.alipay_btn text:nth-child(2){
  border: 1px solid #0d8d37;
}