/* pages/redeem/redeem.wxss */
@import '../footer/footer.wxss';

.fixed_top {
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 100;
}

.i_header {
  padding: 10rpx 26rpx;
  /* height: 90rpx; */
  display: flex;
  align-items: center;
  justify-content: space-between;
  /* position: absolute; */
  background: #f9f9f9;
  /* border-bottom: 2rpx solid #e0e0e0; *//* top: 0; */
  width: 93%;
  z-index: 50;
}

.i_header .serach {
  width: 100%;
  /* width: 580rpx; */
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 50rpx;
  background: #f5f5f5;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 26rpx;
}

.i_header .serach icon {
  color: #333;
  font-size: 40rpx;
  float: left;
  display: flex;
}

.i_header .serach .close {
  color: #7c7c7c;
  font-size: 30rpx;
  font-weight: bold;
  float: right;
}

.i_header .serach input {
  font-size: 24rpx;
  /* width: 500rpx; */
  width: 90%;
  height: 60rpx;
  display: inline-flex;
  padding-left: 16rpx;
}

.i_nav_more {
  position: fixed;
  top: 92rpx;
  z-index: 52;
  height: 0rpx;
  overflow: hidden;
}

.i_nav_more .i_nav_header {
  height: 88rpx;
  width: 100vw;
  /* padding: 0 26rpx; */
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #fff;
  border-bottom: 2rpx solid #e0e0e0;
}

.i_nav_header text {
  font-size: 34rpx;
  color: #444;
  padding-left: 26rpx;
}

.i_nav_header icon {
  display: inline-flex;
  align-content: center;
  padding-right: 26rpx;
}

.i_nav_more .i_downList {
  /* height: 377rpx; */
  background: #fff;
  overflow: hidden;
}

.i_downList .i_down {
  /* border: 2rpx solid transparent; */
  border-right: 2rpx solid #e0e0e0;
  border-bottom: 2rpx solid #e0e0e0;
  width: 25%;
  text-align: center;
  float: left;
  padding: 40rpx 0;
  box-sizing: border-box;
}

.i_down image {
  width: 68rpx;
  height: 68rpx;
  vertical-align: middle;
}

.i_down text {
  font-size: 26rpx;
  color: #444;
  display: block;
  margin-top: 16rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.down .red {
  font-size: 16rpx;
  color: #ff8d97;
  position: absolute;
  right: -4rpx;
  top: -3rpx;
}

.scroll_nav {
  height: 86rpx;
  background: #f9f9f9;
}

.scroll_nav .nav_list {
  height: 86rpx;
  width: 100%;
  white-space: nowrap;
}

.scroll_nav .nav_list .nav_block {
  display: inline-block;
  padding: 16rpx 30rpx;
  position: relative;
}

.scroll_nav .nav_list .nav_block text {
  font-size: 30rpx;
  font-weight: bold;
  color: #222;
}

.scroll_nav .nav_list .nav_block .active_nav {
  color: #ff8d97;
}

.scroll_nav .nav_list .nav_block .iconfont {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: -6rpx;
}

.red_body {
  /* background: #fff; */
  height: 100%;
  /* padding-top: 90rpx; *//* position: absolute; */
}

.red_fifter {
  background: #fff;
  display: flex;
}

.red_fifter .fifter_sort {
  width: 25%;
  line-height: 68rpx;
}

.red_fifter .fifter_brand {
  border-top: 2rpx solid #e4e4e4;
  border-bottom: 2rpx solid #fff;
}

.red_fifter .fifter_brand_active {
  color: #eb3d6e;
}

.red_fifter view:last-child {
  border-right: none;
}

.red_fifter view text {
  font-size: 28rpx;
  color: #666;
}

.fifter_sort {
  display: flex;
  justify-content: center;
  align-items: center;
  min-height: 68rpx;
}

.fifter_sort .iconfont {
  color: #999;
}

.fifter_sort .fifter_icon {
  width: 40rpx;
  min-height: 68rpx;
  position: relative;
}

.fifter_icon .iconfont {
  font-size: 40rpx;
  position: absolute;
  transform: translate(-50%, -50%);
}

.fifter_sort .sort_asc {
  top: 58%;
  left: 50%;
}

.fifter_sort .sort_desc {
  top: 52%;
  left: 50%;
}

.fifter_sort .icon_active {
  color: #fa7682;
}

.fifter_sort text {
  /* margin-left: 16rpx; */
}

.red_body  .fifter_brand_more_bg {
  margin-top: -20rpx;
  position: fixed;
  top: 108rpx;
  z-index: 50;
  background: rgba(0, 0, 0, 0.4);
  width: 100%;
  height: 100%;
}

.fifter_brand_more {
  background: #fff;
  z-index: 60;
  position: fixed;
  top: 76rpx;
}

.fifter_brand_more checkbox-group {
  padding-left: 20px;
}

.fifter_brand_more label {
  width: 340rpx;
  display: inline-flex;
  justify-content: flex-start;
  align-items: flex-end;
  padding: 22rpx 0;
  position: relative;
}

.fifter_brand_more text {
  color: #323232;
  font-size: 28rpx;
}

.fifter_brand_more .chose_text {
  border-bottom: 2rpx solid #ff8d97;
}

.fifter_brand_more  .chose_default {
  border-bottom: 2rpx solid #fff;
}

.fifter_brand_more .fifter_brand_btn {
  box-shadow: 0rpx -4rpx 20rpx -6rpx #a2a2a2;
  height: 75rpx;
  display: flex;
}

.fifter_brand_btn button {
  width: 50%;
  height: 75rpx;
  color: #2b2b2b;
  background: #fff;
  font-size: 30rpx;
  font-weight: bold;
  line-height: 75rpx;
  border-radius: 0;
  margin: 0;
}

.fifter_brand_btn .btn_red {
  background: #ff8d97;
  color: #fff;
}

.fifter_brand_btn button::after {
  border: none;
}

/* 筛选 */

.fifter_more_bg {
  position: fixed;
  top: 0rpx;
  z-index: 199;
  background: rgba(0, 0, 0, 0.4);
  width: 100%;
  height: 100%;
}

.fifter_more {
  position: fixed;
  width: 640rpx;
  height: 100%;
  background: #f2f2f2;
  top: 0;
  right: -640rpx;
  z-index: 200;
}

.fifter_more .fifter_price {
  padding-top: 36rpx;
  height: 155rpx;
  background: #fff;
  padding-right: 26rpx;
  margin-bottom: 20rpx;
  z-index: 200;
}

.fifter_price view {
  margin-left: 30rpx;
  height: 50rpx;
  line-height: 50rpx;
  display: flex;
  align-items: center;
  margin-top: 30rpx;
}

.fifter_price input {
  width: 270rpx;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 50rpx;
  background: #f1f2f6;
  display: inline-block;
  text-align: center;
  font-size: 26rpx;
}

.fifter_price .input-placeholder {
  color: #c3c3c3;
  font-size: 26rpx;
  text-align: center;
}

.fifter_price view icon {
  line-height: 50rpx;
  color: #000;
  margin: 0 10rpx;
}

.fifter_price text {
  font-size: 30rpx;
  color: #333;
  margin-left: 30rpx;
  display: block;
}

.fifter_more .fifter_other {
  background: #fff;
  padding-bottom: 20rpx;
}

.fifter_other .fifter_info {
  padding-top: 30rpx;
}

.fifter_info .fifter_head {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 26rpx;
}

.fifter_head text {
  font-size: 30rpx;
  color: #333;
}

.fifter_head view {
  display: inline-flex;
  align-items: center;
}

.fifter_head view text {
  color: #939393;
  margin-right: 10rpx;
}

.fifter_head view icon {
  display: inline-flex;
  align-items: center;
  font-size: 26rpx;
  color: #939393;
}

.fifter_head view .icon_open {
  transform: rotate(180deg);
}

.fifter_options {
  margin-top: 20rpx;
  padding-left: 26rpx;
}

.fifter_options_hidden {
  height: 80rpx;
  overflow: hidden;
}

.fifter_options text {
  width: 126rpx;
  height: 50rpx;
  line-height: 50rpx;
  padding: 0 16rpx;
  text-align: center;
  font-size: 24rpx;
  color: #6c6c6c;
  background: #f1f2f5;
  border-radius: 50rpx;
  display: inline-block;
  margin: 10rpx 0;
  margin-right: 45rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.fifter_options .chose_text {
  background: #ff8d97;
  color: #fff;
}

.fifter_more .fifter_more_btn {
  box-shadow: 0rpx -4rpx 20rpx -6rpx #a2a2a2;
  height: 75rpx;
  display: flex;
  position: absolute;
  bottom: 0;
  width: 100%;
}
.fifter_more .fifter_more_btn_height{
  bottom: 68rpx;
}
.fifter_more_btn button {
  width: 50%;
  height: 75rpx;
  color: #2b2b2b;
  background: #fff;
  font-size: 30rpx;
  line-height: 75rpx;
  border-radius: 0;
  margin: 0;
}

.fifter_more_btn .btn_red {
  background: #ff8d97;
  color: #fff;
}

.fifter_more_btn button::after {
  border: none;
}

.red_body .editDel {
  height: 92rpx;
  background: #fff;
  border-bottom: 1px solid #f2f2f2;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  padding-right: 26rpx;
}

.editDel button {
  margin: 0;
  color: #ff8d97;
  background: transparent;
  border-radius: 32rpx;
  border: 3rpx solid #ff8d97;
}

.editDel button::after {
  border: 2rpx solid transparent;
}

.head_top {
  height: 148rpx;
  background: transparent;
  z-index: 10;
}

.red_list {
  overflow: hidden;
  padding: 0 20rpx;
  background: #fff;
}

.red_list_null {
  text-align: center;
  font-size: 30rpx;
  /* font-weight: bold; */
  padding: 20rpx 0;
}

.red_list .chose_date {
  color: #686868;
  font-size: 28rpx;
  margin-left: 26rpx;
  font-family: 'DIN';
}

/* 订单商品信息 */

.red_list .red_pro {
  /* padding: 0 26rpx;  */
  width: 346rpx;
  background: #fff;
  text-align: center;
  float: left;
  margin-bottom: 24rpx;
  /* height: 620rpx; */
  margin-top: 16rpx;
}

.red_list .red_pro:nth-child(odd) {
  float: left;
}

.red_list .red_pro:nth-child(even) {
