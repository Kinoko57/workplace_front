  align-items: center;
}
.iphone_x_padding {
  padding-bottom: 34px;
}
.iphone_x_margin{
  margin-bottom: 34px;
}
.model_bg {
  position: fixed;
  z-index: 1000;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.3);
}

.model_style {
  position: fixed;
  z-index: 1001;
  background: #fff;
  width: 100%;
  border-top-left-radius: 40rpx;
  border-top-right-radius: 40rpx;
  /* bottom: 0; */
}

.model_style .model_title {
  padding: 40rpx 0;
  text-align: center;
  color: #333;
  font-size: 30rpx;
}

.model_style .model_body {
  padding: 0 38rpx;
  color: #333;
  font-size: 26rpx;
}

.share_model {
  height: 40vh;
  background: #f7f7f7;
  top: calc(100% - 40vh);
  animation: shareSheet 150ms ease-out;
  /* background: rgba(255,255,255,.8); */
  background: rgba(247,247,247);
}

.share_model .model_body .share_btn_block {
  display: flex;
  align-items: center;
  justify-content: flex-start;
  margin-top: 30rpx;
}

.share_model .model_body .share_btn_block .share_block {
  margin-right: 36rpx;
  font-weight: normal;
  margin: 0;
  width: 30%;
}

.share_model .model_body .share_btn_block .share_block image {
  width: 100rpx;
  height: 100rpx;
  background: #fff;
  border-radius: 50%;
  margin-bottom: 22rpx;
}

.share_model .model_body .share_btn_block .share_block text {
  font-size: 24rpx;
  color: #333;
  display: block;
}

.model_style  .model_footer {
  margin-top: 40rpx;
}
.model_style  .model_footer .btn_tips{
  color: #999999;
  font-size: 22rpx;
  text-align: center;
  margin-bottom: 10rpx;

}

.model_style  .model_footer .cancel_btn {
  width: 100%;
  background: #fff;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 30rpx;
  color: #333;
}
.model_style  .model_footer .sure_btn{
  width: 60%;
  color: #fff;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 30rpx;
  margin: 0 auto;
  border-radius: 50rpx;
}
@keyframes shareSheet {
  from {
    top: 100%;
  }

  to {
    top: calc(100% - 40vh);
  }
}

/* 格式化checkbox */
checkbox .wx-checkbox-input {
  border-radius: 50%;
  width: 34rpx;
  height: 34rpx;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border: 1px solid transparent;
  background: #e13816;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  border-radius: 50%;
  width: 34rpx;
  height: 34rpx;
  line-height: 34rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}
/* 滚动条样式隐藏 */
::-webkit-scrollbar {
  width:0;
  height:0;
  color:transparent;
}


