/* pages/proList/proList.wxss */

page {
  background: #f6f6f6;
}

.list_bg {
}

.list_bg .list_body {
}

/* 搜索和筛选 */

.list_bg .list_body .list_body_head {
  background: #fff;
  margin-bottom: 12rpx;
}

.list_bg .list_body .list_body_head .list_search {
  padding: 10rpx 26rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background: #fff;
  width: 93%;
  z-index: 50;
}

.list_bg .list_body .list_body_head .list_search .search {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 50rpx;
  background: #f5f5f5;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 26rpx;
}

.list_bg .list_body .list_body_head .list_search .search icon {
  color: #333;
  font-size: 34rpx;
  float: left;
  display: flex;
}

.list_bg .list_body .list_body_head .list_search .search .close {
  color: #7c7c7c;
  font-size: 30rpx;
  font-weight: bold;
  float: right;
}

.list_bg .list_body .list_body_head .list_search .search input {
  font-size: 24rpx;
  /* width: 500rpx; */
  width: 84%;
  height: 60rpx;
  /* display: inline-flex; */
  padding-left: 16rpx;
}

.list_bg .list_body .list_body_head  .list_fifter {
  display: flex;
  align-items: center;
  height: 84rpx;
  width: 100%;
  color: #333;
}

.list_bg .list_body .list_body_head  .list_fifter .fifter_complex {
  width: 20%;
  display: inline-flex;
  align-items: center;
  justify-content: center;
}

.list_bg .list_body .list_body_head  .list_fifter .fifter_complex text {
  font-size: 30rpx;
}

.list_bg .list_body .list_body_head  .list_fifter .fifter_complex .fifter_icon {
  position: relative;
  height: 84rpx;
  width: 30rpx;
  margin-left: 6rpx;
}

.list_bg .list_body .list_body_head  .list_fifter .fifter_complex .fifter_icon .iconfont {
  font-size: 16rpx;
  display: inline-block;
}

.list_bg .list_body .list_body_head  .list_fifter .fifter_complex .fifter_icon .asc_icon {
  position: absolute;
  top: -2%;
}

.list_bg .list_body .list_body_head  .list_fifter .fifter_complex .fifter_icon .desc_icon {
  position: absolute;
  bottom: 27%;
}

.list_bg .list_body .list_pro_body {
}

.list_bg .list_body .list_pro_body .list_prolist {
  overflow: hidden;

}
.list_bg .list_body .list_pro_body .list_prolist .list_prolist_blocks{
  overflow: hidden;
  /* column-count: 2; */
  /* 瀑布流 */
  /* -moz-column-count:2;
	Firefox
  -webkit-column-count:2;
	Safari 和 Chrome
  column-count:2;
	-moz-column-gap:10rpx;
	-webkit-column-gap:10rpx;
	column-gap:10rpx; */
}
.list_bg .list_body .list_pro_body .list_prolist .list_pro {
  background: #fff;
  margin-bottom: 10rpx;
  width: 370rpx;
  float: left;
  /* height: 546rpx; */
}
.list_bg .list_body .list_pro_body .list_prolist .list_pro:nth-child(2n+1) {
  float: left;
}

.list_bg .list_body .list_pro_body .list_prolist .list_pro:nth-child(2n+2) {
  float: right;
}
.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_img{
  position: relative;
  width: 370rpx;
  height: 370rpx;
}
.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_img .pro_icon{
  position: absolute;
  width: 80rpx;
  height: 80rpx;
}
.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_img .pro_icon_left_top{
  top: 0;
  left: 0;
}
.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_img .pro_icon_right_top{
  top: 0;
  left: 0;
}
.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_img .pro_icon_left_bottom{
  top: 0;
  left: 0;
}
.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_img .pro_icon_right_bottom{
  top: 0;
  left: 0;
}
.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_img .pro_main_image {
  width: 100%;
  height: 100%;
}
.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_img .pro_min_frame{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_middle {
  padding: 0 14rpx;
}

.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_middle .pro_title {
  color: #333;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-top: 20rpx;
  margin-bottom: 6rpx;
}

.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_middle .pro_desc {
  color: #999;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_middle .pro_tags{
  height: 30rpx;
}
.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_middle .pro_tags .pro_tab_block{
  height: 30rpx;
  border-radius: 4rpx;
  padding:0 6rpx;
  font-size: 20rpx;
  margin-right: 10rpx;
  display: inline-block;
  border: 1px solid #DC0000;
  color: #DC0000;
}
.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_middle .pro_tags .pro_tab_block_null{
  border: 1px solid #fff;
  color: #fff;
}
.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_middle .pro_tags .pro_tab_block:last-child{
  margin-right: 0;
}
.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_bottom {
  padding: 0 14rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 20rpx;
  margin-bottom: 16rpx;
}
.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_bottom_margin{
  padding: 14rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 20rpx;
}
.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_bottom .pro_price {
  font-size: 30rpx;
}

.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_bottom .pro_price text {
  font-size: 24rpx;
}

.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_bottom .pro_bottom_right {
  font-size: 18rpx;
}

.list_bg .list_body .list_pro_body .list_prolist .list_pro .pro_bottom .pro_bottom_right .price_market {
  margin-right: 6rpx;
}

.list_bg .list_body .list_pro_body .list_prolist .list_bottom {
  overflow: hidden;
}

/* 筛选 */

.fifter_more_bg {
  position: fixed;
  top: 0rpx;
  z-index: 199;
  background: rgba(0, 0, 0, 0.4);
  width: 100%;
  height: 100%;
}

.fifter_more {
  position: fixed;
  width: 640rpx;
  height: 100%;
  background: #f2f2f2;
  top: 0;
  right: -640rpx;
  z-index: 200;
}

.fifter_more .fifter_price {
  padding-top: 36rpx;
  height: 155rpx;
  background: #fff;
  padding-right: 26rpx;
  margin-bottom: 20rpx;
  z-index: 200;
}

.fifter_price view {
  margin-left: 30rpx;
  height: 50rpx;
  line-height: 50rpx;
  display: flex;
  align-items: center;
  margin-top: 30rpx;
}

.fifter_price input {
  width: 270rpx;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 50rpx;
  /* background: #f1f2f6; */
  display: inline-block;
  text-align: center;
  font-size: 26rpx;
  border:2rpx solid rgba(228,228,228,1);
}
.fifter_price .activity_input{
  border:2rpx solid rgba(255,0,0,1);
}

.fifter_price .input-placeholder {
  color: #c3c3c3;
  font-size: 26rpx;
  text-align: center;
}

.fifter_price view icon {
  /* line-height: 50rpx; */
  color: #666;
  margin: 0 10rpx;
}

.fifter_price text {
  font-size: 30rpx;
  color: #333;
  margin-left: 30rpx;
  display: block;
}
.fifter_body{
  background: #fff;
  margin-top: 20rpx;
  padding: 30rpx ;
}
.fifter_body .fifter_brand{

}
.fifter_body .fifter_brand .brand_list{
  font-size:24rpx;
  font-weight:400;
  overflow: hidden;
  margin: 16rpx 0;
}
.fifter_body .fifter_brand .brand_list .brand_block{
  color:rgba(128,128,128,1);
  border-radius: 50rpx;
  padding: 10rpx 22rpx;
  border:2rpx solid rgba(228,228,228,1);
  text-align: center;
  float: left;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
  max-width: 200rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.fifter_body .fifter_brand .brand_list .brand_block_active{
  border:1px solid rgba(255,0,0,1);
  color: rgba(255,0,0,1);
}
.fifter_more .fifter_other {
  background: #fff;
  padding-bottom: 20rpx;
}

.fifter_other .fifter_info {
  padding-top: 30rpx;
}

.fifter_info .fifter_head {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 26rpx;
}

.fifter_head text {
  font-size: 30rpx;
  color: #333;
}

.fifter_head view {
  display: inline-flex;
  align-items: center;
}

.fifter_head view text {
  color: #939393;
  margin-right: 10rpx;
}

.fifter_head view icon {
  display: inline-flex;
  align-items: center;
  font-size: 26rpx;
  color: #939393;
}

.fifter_head view .icon_open {
  transform: rotate(180deg);
}

.fifter_options {
  margin-top: 20rpx;
  padding-left: 26rpx;
}

.fifter_options_hidden {
  height: 80rpx;
  overflow: hidden;
}

.fifter_options text {
  width: 126rpx;
  height: 50rpx;
  line-height: 50rpx;
  padding: 0 16rpx;
  text-align: center;
  font-size: 24rpx;
  color: #6c6c6c;
  background: #f1f2f5;
  border-radius: 50rpx;
  display: inline-block;
  margin: 10rpx 0;
  margin-right: 45rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.fifter_options .chose_text {
  background: #ff4e79;
  color: #fff;
}

.fifter_more .fifter_more_btn {
  box-shadow: 0rpx -4rpx 20rpx -6rpx #a2a2a2;
  height: 75rpx;
  display: flex;
  position: absolute;
  bottom: 0;
  width: 100%;
}
.fifter_more .fifter_more_btn_height{
  bottom: 68rpx;
}
.fifter_more_btn button {
  width: 50%;
  height: 75rpx;
  color: #2b2b2b;
  background: #fff;
  font-size: 30rpx;
  border-radius: 0;
  margin: 0;
}

.fifter_more_btn .btn_red {
  background: linear-gradient(to right, #f8721e, #e84928);
  color: #fff;
}

.fifter_more_btn button::after {
  border: none;
}