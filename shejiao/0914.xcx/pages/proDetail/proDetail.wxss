  justify-content: space-between;
}

.detail_bg .pro_body .pro_info .pro_info_top .pro_news .pro_news_left {
  width: 620rpx;
  padding: 0 20rpx;
}

.detail_bg .pro_body .pro_info .pro_info_top .pro_news .pro_news_left .pro_price {
  display: flex;
  align-items: center;
}

.detail_bg .pro_body .pro_info .pro_info_top .pro_news .pro_news_left .pro_price .price_icon {
  font-size: 36rpx;
}

.detail_bg .pro_body .pro_info .pro_info_top .pro_news .pro_news_left .pro_price .price {
  font-size: 60rpx;
}

.detail_bg .pro_body .pro_info .pro_info_top .pro_news .pro_news_left .pro_price .price_memeber {
  position: relative;
  margin-left: 20rpx;
  height: 32rpx;
  display: inline-flex;
  align-items: center;
}

.detail_bg .pro_body .pro_info .pro_info_top .pro_news .pro_news_left .pro_price .price_memeber text {
  font-size: 20rpx;
  min-width: 90rpx;
  border: 2rpx solid;
  padding: 0 10rpx;
  height: 30rpx;
  display: inline-block;
  line-height: 30rpx;
}

.detail_bg .pro_body .pro_info .pro_info_top .pro_news .pro_news_left .pro_price .price_memeber text:before {
  box-sizing: content-box;
  width: 0px;
  height: 0px;
  position: absolute;
  top: 8rpx;
  left: -14rpx;
  padding: 0;
  /* border: 10rpx solid #fff; */
  border-right: 10rpx solid #fff;
  border-top: 10rpx solid transparent;
  border-bottom: 10rpx solid transparent;
  border-left: 10rpx solid transparent;
  display: block;
  content: '';
  z-index: 12;
}

.detail_bg .pro_body .pro_info .pro_info_top .pro_news .pro_news_left .pro_price .price_memeber text:after {
  box-sizing: content-box;
  width: 0px;
  height: 0px;
  position: absolute;
  top: 10rpx;
  left: -14rpx;
  padding: 0;
  /* border: 9rpx solid #e13816; */
  border-right: 8rpx solid #e13816;
  border-top: 8rpx solid transparent;
  border-bottom: 8rpx solid transparent;
  border-left: 8rpx solid transparent;
  display: block;
  content: '';
  z-index: 10;
}

