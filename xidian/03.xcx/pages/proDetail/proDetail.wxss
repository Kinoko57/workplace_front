  display: flex;
  align-items: flex-end;
  margin-left: 9rpx;
}

.spec_pro_price .spec_pro_inter_img .iconfont {
  font-size: 30rpx;
  display: 0d8d37-flex;
  color: #0d8d37;
  font-weight: bold;
}
.spec_pro_price text {
  font-size: 24rpx;
  color: #b8b8b8;
}

.spec_pro_code {
  font-size: 24rpx;
  color: #9d9d9d;
  margin-left: 186rpx;
  margin-top: 6rpx;
  display: flex;
  justify-content: space-between;
}

.spec_pro_code .spec_pro_orig {
  font-size: 24rpx;
  color: #b8b8b8;
  text-decoration: line-through;
}

.spec_head .spec_close {
  position: absolute;
  right: 26rpx;
  top: 6rpx;
}

.spec_body {
  padding: 0 26rpx;
  overflow: hidden;
  width: 700rpx;
  height: calc(60vh - 160px);
}

.spec_title {
  color: #9d9d9d;
  /* display: flex;
