.tab-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 49px;
  background: white;
  display: flex;
  padding-bottom: env(safe-area-inset-bottom);
}

.tab-bar-border {
  background-color: rgba(0, 0, 0, 0.33);
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 2rpx;
  transform: scaleY(0.5);
}

.tab-bar-item {
  /* flex: 1; */
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  position: relative;
  width: 25%;
}
.tab-bar-item-fixed {
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  position: absolute;
  top: 26%;
  height: 150rpx;
  width: 160px;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 25%;
  z-index: 100;
}
.tab-bar-item-fixed .placeholder-view{
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  height: 49px;
}
.tab-bar-item image {
  width: 54rpx;
  height: 54rpx;
  margin-bottom: 6rpx;
}
.tab-bar-item .icon_top{
  width: 150rpx;
  height: 150rpx;
  position: absolute;
}

.tab-bar-item view {
  font-size: 20rpx;
}
