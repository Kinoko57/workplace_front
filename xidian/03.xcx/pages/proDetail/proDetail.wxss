  width: 100%;
  color: #fff;
  font-size: 32rpx;
  border-radius: 44rpx;
  height: 88rpx;
  display: flex;
  background: linear-gradient(to right, #20b351, #0d8d37);
  align-items: center;
  justify-content: center;
}
.spec_btn .surebtn button {
  width:690rpx;
  background: none;
  border-radius: 44rpx;
  height: 88rpx;
}
.spec_btn .surebtn_null {
  width: 100%;
  background: #f5f5f5;
  color: #999;
  font-size: 32rpx;
  border-radius: 44rpx;
  height: 88rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
