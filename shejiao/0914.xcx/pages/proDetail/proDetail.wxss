}

.detail_bg .pro_body .pro_head .pro_head_body .top_icon {
  background: rgba(0, 0, 0, 0.4);
  border-radius: 50rpx;
  color: #fff;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  position: relative;
}
.detail_bg .pro_body .pro_head .pro_head_body .top_icon .num_icon{
  left: 80%;
