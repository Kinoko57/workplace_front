/* pages/myCar/myCar.wxss */
@import '../footer/footer.wxss';

.my_car {
  font-family: SimHei;
  background: #f2f2f2;
  overflow-x: hidden;
  overflow-y: hidden;
}

.car_header {
  text-align: center;
  background: #ebebeb;
  height: 86rpx;
  line-height: 86rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
  position: fixed;
  top: 0;
  z-index: 50;
}

.car_header text {
  padding-left: 26rpx;
  color: #515151;
  font-size: 24rpx;
}

.car_header .car_num {
  color: #ff8d97;
  font-size: 26rpx;
  padding-left: 0;
}

.car_header .car_edit {
  float: right;
  padding-right: 26rpx;
  color: #ff8d97;
  font-size: 24rpx;
}

.car_body {
  padding-top: 86rpx;
}

.car_body_null {
  padding-top: 110rpx;
  text-align: center;
}

.car_body_null text {
  font-size: 28rpx;
  color: #555;
  display: block;
}

.car_body_null .btn_red {
  margin-top: 20rpx;
  display: inline-flex;
  height: 66rpx;
  line-height: 66rpx;
  border: none;
  background: #ff8d97;
  color: #fff;
  outline: none;
  font-size: 32rpx;
  border-radius: 20rpx;
}

.car_body .editDel {
  height: 92rpx;
  background: #fff;
  border-bottom: 1px solid #f2f2f2;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  position: fixed;
  top: 86rpx;
  width: 100%;
  z-index: 50;
}

.editDel button {
  margin: 0;
  color: #ff8d97;
  background: transparent;
  border-radius: 32rpx;
  border: 3rpx solid #ff8d97;
  margin-right: 26rpx;
}

.editDel button::after {
  border: 2rpx solid transparent;
}

.car_list {
  /* background: #fff; */
  height: 100%;
}

/* .car_list_height {
  padding-top: 92rpx;
} */

.car_list .chose_date {
  color: #686868;
  font-size: 28rpx;
  margin-left: 26rpx;
  font-family: 'DIN';
}

/* 订单商品信息 */

.car_list .car_pro {
  padding: 10rpx 36rpx;
  height: 260rpx;
  background: #fff;
  display: flex;
  align-items: center;
  margin-bottom: 20rpx;
}

.car_pro .pro_chose {
  float: left;
  display: flex;
  align-items: center;
  height: 262rpx;
}

.car_pro .pro_img {
  float: left;
  /* margin-top: 40rpx; */
  margin-left: 18rpx;
  margin-right: 30rpx;
  position: relative;
}

.pro_img image {
  width: 190rpx;
  height: 190rpx;
}

.pro_img .pro_tips {
  width: 190rpx;
  height: 52rpx;
  background: rgba(0, 0, 0, 0.6);
  line-height: 52rpx;
  text-align: center;
  position: absolute;
  bottom: 0;
}

.pro_tips text {
  color: #fff;
  font-size: 24rpx;
}

.car_pro .pro_info {
  /* width: 100%; */
  width: 400rpx;
  float: left;
}

.pro_info .pro_title {
  font-size: 24rpx;
  color: #444;
  /* margin-top: 50rpx; */
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.pro_info .pro_title .pro_price_third{
  /* color: #ff8d97;
  font-size: 22rpx; */
  height: 24rpx;
  line-height: 24rpx;
  border: 2rpx solid #ff8d97;
  font-size: 20rpx;
  color: #ff8d97;
  border-radius: 5rpx;
  display: inline-block;
  text-align: center;
  padding: 0 6rpx;
  margin-right:4rpx;

}
.pro_spec_info {
  font-size: 24rpx;
  color: #7c7c7c;
}

.pro_info .pro_spec {
  /* width: 490rpx; */
  overflow: hidden;
  /* margin-top: 14rpx; *//* height: 64rpx; */
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.pro_spec .pro_price {
  font-family: 'DIN';
  width: 240rpx;
  /* display: flex;
  align-items: center; */
}

.pro_price .peo_nowprice {
  color: #ff8d97;
  font-size: 30rpx;
  font-weight: bold;
  /* margin-bottom: -10rpx; */
}

.pro_price .pro_integral {
  font-size: 30rpx;
  font-weight: bold;
  color: #ff8d97;
  /* font-weight: bold; *//* margin-left: 8rpx; */
}

.pro_price .pro_inter_img {
  display: inline-flex;
  align-items: flex-end;
  /* margin-left: 9rpx; *//* margin-right: 30rpx; */
}

.pro_price .pro_inter_img .iconfont {
  display: inline-flex;
  font-size: 30rpx;
  font-weight: bold;
  color: #ff8d97;
}

.pro_price .pro_orig {
  font-size: 22rpx;
  color: #7c7c7c;
  text-decoration: line-through;
}

.pro_spec .pro_icon {
  margin-right: 4rpx;
  display: flex;
  border: 1px solid #ccc;
  border-radius: 6rpx;
}

.pro_icon button {
  width: 44rpx;
  height: 44rpx;
  line-height: 42rpx;
  padding: 0 22rpx;
  margin: 0;
  color: #9f9f9f;
  font-size: 38rpx;
  display: flex;
  justify-content: center;
  border-radius: 0;
  background: #f3f3f3;
}

.pro_icon  button::after {
  border: none;
}

.pro_icon input {
  width: 70rpx;
  height: 44rpx;
  min-height: 44rpx;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
  padding: 0;
  font-size: 30rpx;
  /* font-weight: bold; */
  color: #565656;
  text-align: center;
  display: flex;
  align-items: center;
  font-family: 'DIN';
}

/* 购物车推荐商品 */

.recom_info {
}

.i_pro_content_null {
  font-size: 25rpx;
  color: #555;
  text-align: center;
  background: #f2f2f2;
  padding: 30px 0;
}

.recom_info .i_title {
  font-size: 25rpx;
  color: #bebebe;
  text-align: center;
  display: block;
  height: 99rpx;
  line-height: 99rpx;
  background: #f2f2f2;
}

.recom_list {
  background: #f2f2f2;
  overflow: hidden;
}

.recom_list .i_pro {
  width: 370rpx;
  height: 550rpx;
  float: left;
  background: #fff;
  /* padding: 0 26rpx; *//* margin-right: 7rpx; */
  margin-bottom: 13rpx;
  position: relative;
}

.recom_list .i_pro:nth-child(even) {
  float: right;
}

.recom_list .i_pro .pro_img {
  /* margin-top: 40rpx;
  margin-left: 18rpx;
  margin-right: 30rpx; */
  position: relative;
}

.recom_list .pro_img image {
  width: 370rpx;
  height: 370rpx;
}

.recom_list .i_pro .pro_title {
  width: 320rpx;
  font-size: 24rpx;
  color: #333;
  margin-top: 20rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  height: 70rpx;
  padding: 0 26rpx;
}

.recom_list .pro_title  .pro_level {
  width: 80rpx;
  border: 2rpx solid #ff8d97;
  border-radius: 16rpx;
  display: inline-flex;
  align-items: center;
  margin-right: 6rpx;
  padding: 0 4rpx;
  justify-content: center;
}

.recom_list .pro_level .iconfont {
  color: #ff8d97;
  display: inline-flex;
}

.recom_list .pro_level text {
  color: #ff8d97;
  font-size: 22rpx;
}

.recom_list .i_pro .pro_price {
  margin-top: 10rpx;
  font-family: 'DIN';
  /* width: 280rpx; */
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 16rpx;
}

.recom_list .pro_price .pro_flex {
  display: inline-flex;
  align-items: center;
}

.recom_list .pro_price .pro_flex .iconfont {
  color: #ff8d97;
  font-size: 30rpx;
  font-weight: bold;
  display: inline-flex;
}

.recom_list .pro_price .i_level {
  position: relative;
}

.recom_list .pro_price .i_level image {
  width: 38rpx;
  height: 41rpx;
}

.recom_list .pro_price .i_level text {
  color: #fff;
  position: absolute;
  left: 50%;
  top: 35%;
  transform: translate(-50%, -50%);
  font-size: 18rpx;
  font-weight: bold;
  font-family: 'DIN';
}

.recom_list .pro_price .peo_nowprice {
  color: #ff8d97;
  font-size: 30rpx;
  font-weight: bold;
}

.recom_list .pro_price .pro_integral {
  font-size: 30rpx;
  font-weight: bold;
  color: #ff8d97;
}

.recom_list .pro_price .pro_inter_img {
  display: inline-flex;
  align-items: flex-end;
  /* margin-left: 10rpx; */
}

.recom_list .pro_price .pro_inter_img .iconfont {
  font-size: 30rpx;
  font-weight: bold;
  color: #ff8d97;
  display: inline-flex;
}

/* .recom_list .pro_price .pro_inter_img image {
  width: 24rpx;
  height: 20rpx;
} */

.recom_list .pro_price .pro_orig {
  font-size: 22rpx;
  color: #7c7c7c;
  text-decoration: line-through;
}

.order-car {
  height: 84rpx;
  line-height: 84rpx;
  /* background: #fff; */
  text-align: center;
  margin-bottom: 100rpx;
}

.order-car text {
  font-size: 23.97rpx;
  color: #bebebe;
}

/* 空白的块，解决底部导航占领高度 */

.null_height {
  height: 100rpx;
}

.car_score {
  width: 100%;
  height: 74rpx;
  position: fixed;
  bottom: 100rpx;
  border-top: 1px solid #edd9b0;
  border-bottom: 1px solid #edd9b0;
  background: #fff3db;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 26rpx;
  color: #4a4a4a;
  z-index: 20;
}

.car_score .score {
  padding-left: 16rpx;
}

.score .total_score {
  color: #ff8d97;
  font-family: 'DIN';
}

.car_score .shipping {
  padding-left: 16rpx;
  padding-right: 16rpx;
}

.shipping .total_score {
  color: #ff8d97;
  font-family: 'DIN';
}

.shipping .join_total {
  margin-left: 10rpx;
}

.car_edit_more {
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: fixed;
  bottom: 0;
  width: 100%;
  background: #fff;
  z-index: 50;
}

.car_edit_more .allCheck {
  margin-left: 26rpx;
  font-size: 26rpx;
  color: #555;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.allCheck .car_pro_total {
  font-family: 'DIN';
  font-size: 30rpx;
  /* font-weight: bold; */
  display: flex;
  align-items: center;
  color: #484848;
  margin-left: 27rpx;
}

.car_pro_total text {
  font-size: 42rpx;
  color: #ff8d97;
  font-family: 'DIN';
  align-items: flex-end;
}

.car_edit_more button {
  float: right;
  min-width: 244rpx;
  height: 100rpx;
  background: #ff8d97;
  color: #fff;
  font-size: 34rpx;
  line-height: 100rpx;
  border-radius: 0;
  margin: 0;
}

checkbox .wx-checkbox-input {
  border-radius: 50%;
  width: 34rpx;
  height: 34rpx;
  /* border: 1px solid #d8d8d8;
  background: #484848; */
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border: 1px solid transparent;
  background: #ff8d97;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  border-radius: 50%;
  width: 34rpx;
  height: 34rpx;
  line-height: 34rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}
