/* pages/myVip/myVip.wxss */

page {
  background: #0d4dae;
}

.my_vip {
  background: #0d4dae;
  width: 100%;
  height: 100%;
}

.vip_header {
  text-align: center;
  background: #f7f7f7;
  height: 94rpx;
  line-height: 94rpx;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 50;
}

.vip_header  icon {
  float: left;
  padding-left: 26rpx;
  color: #5d5d5d;
}

.vip_header text {
  color: #393939;
  font-size: 38rpx;
  margin-left: -26rpx;
}

.vip_bg .vip_bg_01 {
  position: absolute;
  width: 100%;
  height: 100%;
}

.vip_bg .vip_top_img {
  position: absolute;
  width: 570rpx;
  height: 420rpx;
  top: 700rpx;
  left: 52%;
  transform: translateX(-50%);
  z-index: 30;
}

.vip_title {
  display: flex;
  /* justify-content: center; */
  text-align: center;
  padding: 20rpx 0;
}

.vip_title text {
  width: 50%;
  color: #fff;
  font-size: 38rpx;
}

.vip_body {
  position: absolute;
  /* top: 1060rpx; */
  top: 460rpx;
  width: 100%;
  /* background: #1152b0;  */
  overflow: hidden;
  margin-bottom: 100rpx;
  /* padding-bottom: 40rpx; *//* background: #0d4fcd; */
}

.list_img {
  width: 100%;
  /* margin-bottom: -10rpx; *//* opacity: 0.5; */
}

.vip_list_bg {
  margin-top: -12rpx;
  overflow: hidden;
  /* opacity: 0.5; */
  background-image: url(data:image/png;base64,);
  background-size: 100%;
  background-repeat: repeat-y;
}

.vip_list {
  height: 100%;
  padding: 0 30rpx;
}

.vip_list .chose_date {
  color: #686868;
  font-size: 28rpx;
  margin-left: 26rpx;
  font-family: 'DIN';
}

/* 订单商品信息 */

.vip_list .vip_pro {
  /* height: 262rpx; */
  display: flex;
  align-items: center;
  /* padding: 10rpx 40rpx; */
  padding: 40rpx;
  padding-bottom: 10rpx;
  /* padding-bottom: 70rpx; */
}

.vip_pro .pro_img {
  float: left;
  /* margin-top: 40rpx; *//* margin-left: 18rpx; */
  margin-right: 36rpx;
  position: relative;
}

.pro_img image {
  width: 190rpx;
  height: 190rpx;
}

.pro_img .pro_tips {
  width: 190rpx;
  height: 52rpx;
  background: rgba(0, 0, 0, 0.6);
  line-height: 52rpx;
  text-align: center;
  position: absolute;
  bottom: 0;
}

.pro_tips text {
  color: #fff;
  font-size: 24rpx;
}

.vip_pro .pro_info {
  width: 400rpx;
}

.pro_info .pro_title {
  font-size: 24rpx;
  color: #444;
  /* margin-top: 50rpx; */
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.pro_info .pro_spec {
  overflow: hidden;
  /* margin-top: 20rpx; */
  /* height: 82rpx; */
  /* display: flex;
  align-items: center;
  justify-content: space-between; */
}

.pro_spec .pro_price {
  font-family: 'DIN';
  /* width: 226rpx; */
  margin-top:10rpx;

}

.pro_price .peo_nowprice {
  color: #ff8d97;
  font-size: 30rpx;font-weight: bold;
}

.pro_price .pro_integral {
  font-size: 30rpx;font-weight: bold;
  color: #ff8d97;
  /* font-weight: bold; *//* margin-left: 8rpx; */
}

.pro_price .pro_inter_img {
  display: inline-flex;
  /* margin-left: 9rpx; */
}

.pro_price .pro_inter_img .iconfont {
  font-size: 40rpx;
  display: inline-flex;
  color: #ff8d97;
}

.pro_price .pro_orig {
  /* margin-top: -16rpx; */
  /* height: 30rpx;
  line-height: 30rpx; *//* position: relative; */
}

.pro_price .pro_orig .nowprice {
  font-size: 24rpx;
  color: #b8b8b8;
  /* margin-left: 8rpx; */
}

.pro_price .pro_orig .line {
  text-decoration: line-through;
}

.pro_spec .pro_btn {
  display: flex;
  border-radius: 6rpx;
}

.pro_btn button {
  width: 150rpx;
  /* height: 52rpx; */
  background: #ff8d97;
  color: #fff;
  font-size: 26rpx;
  padding: 0rpx 20rpx;
  line-height: 1.8;
  border-radius: 18rpx;
  margin-right: 0;
  margin-top: -38rpx;
}

.pro_btn button::after {
  border: none;
}
