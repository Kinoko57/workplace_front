.cou_list .cou_info .cou_left {
  width: 76%;
  color: #fff;
  font-size: 22rpx;
  padding-left: 20rpx;
  text-align: left;
  background: #ff8d97;
  height: 160rpx;
}

.cou_list .cou_info .cou_left_get {
  background: #c9c9c9;
}

.cou_info .cou_left .cou_amount {
  font-size: 26rpx;
  margin-top: 18rpx;
  margin-bottom: 4rpx;
}

.cou_info .cou_line {
  border-left: 4rpx dashed #f4f4f4;
  height: 160rpx;
  background: #ff8d97;
}

.cou_info .cou_line_get {
  background: transparent;
}

.cou_amount .price {
  font-size: 40rpx;
  font-weight: bold;
  margin-left: 10rpx;
  margin-right: 20rpx;
  font-family: 'DIN';
}

.cou_amount .type {
  font-size: 30rpx;
}

.cou_info .cou_left .cou_max {
  font-size: 18rpx;
  margin-bottom: 10rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.cou_info .cou_left .cou_time {
  font-size: 18rpx;
}

.cou_info .cou_right {
  color: #fff;
  font-size: 24rpx;
  width: 21%;
  background: #ff8d97;
  height: 160rpx;
}

.cou_info .cou_right_get {
  background: #fff;
}

.cou_info .cou_right .isget {
  color: #c9c9c9;
  font-size: 124rpx;
  margin-top: 13rpx;
}

.cou_info .cou_right text {
  width: 40rpx;
  display: block;
  margin: 0 auto;
  margin-top: 24rpx;
  line-height: 1.2;
}

.sign {
  position: fixed;
  right: 16rpx;
  top: 60%;
  z-index: 100;
}

.sign image {
  width: 88rpx;
  height: 88rpx;
}

.chat {
  z-index: 100;
  position: fixed;
  right: 16rpx;
  bottom: 39%;
  width: 100rpx;
  height: 120rpx;
  margin: 0;
  padding: 0;
  background: transparent;
}

.customerbut--gucon {
  position: fixed !important;
  right: 16rpx !important;
  bottom: 37% !important;
  z-index: 100 !important;
  margin-top: 0 !important;
  width: auto !important;
  height: auto !important;
  margin-right: 0 !important;
}

.customerbut--gucon button {
  padding-right: 0;
}

.chat button image {
  width: 88rpx;
  height: 88rpx;
}

.contact {
  position: fixed;
  width: 180rpx;
  height: 54rpx;
  line-height: 1.9;
  background: #808080;
  left: 16rpx;
  bottom: 10rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 199;
  padding: 0 10rpx;
  border-radius: 26rpx;
}

.contact .iconfont {
  color: #fff;
  margin-right: 10rpx;
}

.contact text {
  font-size: 24rpx;
  color: #fff;
}
