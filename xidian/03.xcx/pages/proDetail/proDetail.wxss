/* pages/flashSalesDetail/flashSalesDetail.wxss */
/* 富文本解析组件 */
@import "/wxParse/wxParse.wxss";
page{
  background-color: #f7f7f7;
}
.flashDetail{
  background: #f2f2f2;
  overflow-x: hidden;
  width: 100%;
  height: 100vh;
  overflow-y: auto;
}
.i_wraper{
  width: 100%;
  height:750rpx;
}
.i_wraper swiper{
  width: 100%;
  height:750rpx;
}
.i_wraper swiper swiper-item{
  width: 100%;
  height:750rpx;
}
.i_wraper swiper swiper-item image{
  width: 750rpx;
  height: 750rpx;
}
.f_content{
  background: #fff;
  padding: 0 0rpx 30rpx;
  margin-bottom: 15rpx;
}

.f_content .f_con_time{
  width: 495rpx;
  height: 97rpx;
  position: relative;
  left: 50%;
  transform: translateX(-50%);
}
.f_content .f_con_time .f_con_time_bg{
  width: 495rpx;
  height: 97rpx;
  position: absolute;
}
.f_content .f_con_time .f_con_time_star{
  position: relative;
  width: 100%;
  height: 96rpx;
  text-align: center;
  padding-top: 18rpx;
}
.f_content .f_con_time .f_con_time_star_l{
 color: #333;
 font-size: 36rpx;
 font-weight: 600;
 display:inline-block;
}
.f_content .f_con_time .f_con_time_star_r{
  display: inline-block;
  font-size: 32rpx;
    color: #fff;
  padding-left: 10rpx;
}
.f_con_time_star_r .sale_time_style{
  width: 52rpx;
  height:52rpx;
  background: #333333;
  text-align: center;
  line-height: 52rpx;
  border-radius: 10rpx;
  vertical-align: middle;
  display:inline-block;
  font-size:24rpx;
  margin:0 5rpx;
}
.f_con_time_star_r .sale_icon_style{
  text-align: center;
  width: 30rpx;
  display: inline-block;
}
.f_content .f_con_tit{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 0 0;
}
/* 添加样式 */
.f_con_remark{
  color: #999;
  font-size: 22rpx;
  padding: 0 20rpx;
  margin-top: 10rpx;
  word-break:break-all;
}
.pro_tag{
  padding: 0 20rpx;
  color: #999;
  font-size: 22rpx;
  margin-top: 15rpx;
}
.pro_tag text{
  display: inline-block;
  margin-bottom: 10rpx;
  border: 1px solid #999;
  height: 36rpx;
  padding: 0 6rpx;
  border-radius: 10rpx;
  line-height:36rpx;
  margin-right:10rpx;
}
.f_content .f_con_tit_text{
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  width: 560rpx;
  padding-left: 20rpx;
  overflow: hidden;
  color: #222;
  font-size: 30rpx;
  word-break: break-all
}
.f_con_tit_collect{
   color: #999;
   font-size: 24rpx;
}
.f_con_tit_collect  .coll_bg{
  background: #f6f6f6;
  border-top-left-radius: 50rpx;
  border-bottom-left-radius: 50rpx;
  height: 50rpx;
  padding: 0 20rpx;
  display: inline-flex;
  align-items: center;
  justify-content: center;
}
.f_con_tit_collect icon{
  display: flex;
  align-items: center;
  margin-right: 4rpx;
  color: #ffa85e;
}
/* .f_con_tit_collect icon.iconcoll1{
  color: #ffa85e;
} */
.f_content .f_con_pri{
  color: #0d8d37;
  overflow: hidden;
  padding: 20rpx 20rpx 0
}
.f_content .f_con_pri .f_con_pri_l{
  float: left;
  font-size: 24rpx;
}
.f_content .f_con_pri .f_con_pri_l text{
  font-size: 44rpx;
}
.f_content .f_con_pri .f_con_pri_c{
  float: left;
  font-size: 26rpx;
  color:#888;
  text-decoration: line-through;
  padding-left: 18rpx;
  padding-top: 16rpx;
  }
.f_content .f_con_pri .f_con_pri_r{
  float: right;
   font-size: 24rpx;
  color:#888;
  padding-top: 18rpx;
}
.pro_spec {
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 20rpx;
  font-size: 26rpx;
  margin-bottom: 15rpx;
  background-color: #fff;
  color: #adadad;
  /* border-top: 1px solid #eae8eb; */
  display: flex;
  justify-content: space-between;
  background: #fff;
}

.pro_spec text {
  color: #777;
  margin-left: 20rpx;
  /* margin-right: -20rpx; */
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.pro_spec .spec_icon {
  /* float: right; */
  font-size: 30rpx;
  font-weight: bold;
  /* width: 110rpx; */
}

.pro_service {
  padding: 0 26rpx;
  font-size: 26rpx;
  color: #444;
  background: #fff;
  margin-top: 10rpx;
}

.pro_service_t {
  height: 60rpx;
  line-height: 60rpx;
  display: flex;
  justify-content: space-between;
}

.pro_service_t .pro_service_t_r {
  color: #979797;
}

.pro_service_t .pro_service_t_r icon {
  margin-left: 16rpx;
  font-size: 24rpx;
  color: #979797;
}

.pro_service_b {
  font-size: 22rpx;
  padding-bottom: 20rpx;
  color: #333;
}

.pro_service_b text {
  display: inline-block;
  padding: 0 10rpx;
}

.spec_icon {
  width: 100rpx;
}

.spec_icon icon:nth-child(1) {
  color: #dcdcdc;
}

.spec_icon icon:nth-child(2) {
  color: #bfbfbf;
}

.spec_icon icon:nth-child(3) {
  color: #a8a8a8;
}

/* 商品介绍 */

.f_proDetail{
  background: #fff;
}
.f_proDetail .f_proDetail_title{
  display: flex;
  justify-content:center;
  align-items: center;
  /* color: #333; */
  color: #0d8d37;
  height: 96rpx;
  font-size: 24rpx;
}
.f_proDetail .f_proDetail_title .xian{
  width:84rpx;
  height: 2rpx;
  background: #ddd; 
  display: inline-block;
  margin: 0 24rpx;
}
.f_proDetail_img{
  width: 100%;
  text-align: center;
  padding-bottom: 80rpx;
}

.pro_btn {
  height: 100rpx;
  position: fixed;
  width: 100%;
  bottom: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background: #f2f2f2;
}

.pro_btn .link_icon {
  display: inline-flex;
  align-items: center;
  background: #fff;
  width: 230rpx;
}

.pro_btn .link_icon .link_icon_block {
  width:60%;
  height: 100rpx;
  color: #666;
  position: relative;
  line-height: 1.2;
  font-weight: normal;
  border-radius: 0;
  font-size: 22rpx;
  background: #fff;
}

.pro_btn .link_icon .link_icon_block .iconfont {
  font-size: 44rpx;
  text-align: center;
  margin-top: 10rpx;
}

.pro_btn .link_icon .link_icon_block text {
  display: block;
  text-align: center;
  font-size: 22rpx;
}

.pro_btn .buy_btn {
  width: 519rpx;
  display: inline-flex;
  align-items: center;
}

.pro_btn .buy_btn button {
  background: transparent;
  border-radius: 0;
  font-weight: normal;
  width: 100%;
  color: #fff;
  font-size: 26rpx;
  height: 100rpx;
  display: inline-flex;
  align-items: center;
  justify-content: center;
}
.pro_btn .buy_btn button text{
  display: block;
  font-size: 20rpx;
}
.pro_btn .buy_btn button .btn_text{
  font-size: 26rpx;
}

.pro_btn .buy_btn .buy {
  background: #0d8d37;
}
.pro_btn .buy_btn .bg_theme_gray{
 background: #f5f5f5;
 color: #999;
}

.pro_btn .buy_btn .share {
  background: #0d8d37;
}

