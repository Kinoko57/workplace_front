@import 'static/iconfont.wxss';

/* 图片垂直居中 */

image {
  vertical-align: middle;
}

.page_footer {
  width: 100%;
  text-align: center;
  color: #666;
  font-size: 26rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin-bottom: 20rpx;
  overflow: hidden;
}
.page_footer_loading{
  width: 100%;
  text-align: center;
  color: #666;
  font-size: 26rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin-bottom: 20rpx;
  overflow: hidden;
}

/* 去除按钮边框 */

button::after {
  border: none;
}
button {
  padding: 0;
  margin:0;
  background: none;
  border: 0;
  border-radius:0;
}
.button-hover{
  background:none;
  box-shadow:0;
  border: 0;
  padding: 0;
  margin: 0;
  border-radius:0;
}
/* 购物车数量角标 */
.num_icon{
  width: 22rpx;
  height: 22rpx;
  line-height: 22rpx;
  background: #fff;
  border: 2rpx solid #e13816;
  border-radius: 50%;
  color: #e13816;
  font-size: 18rpx !important;
  position: absolute;
  left: 70%;
  transform: translateX(-50%);
  top: 8rpx;
  text-align: center;
  padding: 3rpx;
}
/* 主题色-红色 */

.color_theme_red {
  color: #e13816 !important;
}

/* 主题色-灰色 */
.color_theme_gray3 {
  color: #333;
}
.color_theme_gray {
  color: #999;
}

/* 主题背景色-红色 */

.bg_theme_red {
  background: #e13816;
}
/* 主题渐变背景色-红色 */

.bg_theme_linear_red {
  background:linear-gradient(-90deg,rgba(248,45,45,1) 0%,rgba(254,153,153,1) 100%);
}
/* 主题背景色-灰色 */

.bg_theme_gray {
  background: #dedbde;
}

/* 价格下划线 */

.line_through {
  text-decoration: line-through;
}

/* 标签样式 */

.theme_tips {
  height: 27rpx;
  color: #fff;
  padding: 0 4rpx;
  font-size: 20rpx;
  margin-right: 6rpx;
}
.pro_tag_list_style {
