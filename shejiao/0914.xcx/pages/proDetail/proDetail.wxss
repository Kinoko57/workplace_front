/* 富文本解析组件 */
@import "/wxParse/wxParse.wxss";

page {
  background: #f6f6f6;
}

.block_bottom {
  margin-bottom: 20rpx;
}

.detail_bg {
}

.detail_bg .pro_body {
}

.detail_bg .pro_body .pro_head {
  width: 100%;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 199999;
  flex-shrink: 0;
}

.detail_bg .pro_body .pro_head_fixed {
  background: #fff;
}

.detail_bg .pro_body .pro_head .pro_head_body {
  margin-left: 20rpx;
