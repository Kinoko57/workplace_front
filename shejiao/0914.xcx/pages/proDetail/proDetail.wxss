}
.detail_bg .pro_body .pro_head .pro_head_body .iconfont {
  font-size: 40rpx;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  /* color: #333; */
}

/* 商品快速导航 */

.detail_bg .pro_body .pro_tabs {
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: fixed;
  width: 100%;
  z-index: 100;
  background: #fff;
  height: 80rpx;
  box-shadow: 4rpx 8rpx 60rpx 0px rgba(4, 0, 0, 0.08);
  text-shadow: 4rpx 8rpx 60rpx rgba(4, 0, 0, 0.08);
}

.detail_bg .pro_body .pro_tabs view {
  text-align: center;
  font-size: 30rpx;
  width: 25%;
  position: relative;
  height: 80rpx;
  line-height: 80rpx;
}

.detail_bg .pro_body .pro_tabs view .nav_chose {
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
  height: 4rpx;
  width: 80rpx;
}

/* 商品信息 */

