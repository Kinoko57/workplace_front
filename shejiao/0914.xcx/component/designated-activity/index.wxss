}
/* 特定活动的单列表排版 */
.acticity_02 .activity_list_line {

}

.acticity_02 .activity_list_line .activity_block {
  border-radius: 12rpx;
  padding: 20rpx 0;
  padding-bottom: 0;
}

.acticity_02 .activity_list_line .activity_block .pro_img {
  border-top-left-radius: 12rpx;
  border-top-right-radius: 12rpx;
