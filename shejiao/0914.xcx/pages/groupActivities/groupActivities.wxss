/* pages/groupActivities/groupActivities.wxss */
page{
  background: #f6f6f6;
}
.group_body{
  padding-top: 24rpx;
}
.gropu_top{
  position: relative;
  left: 50%;
  top: 0;
  transform: translateX(-50%);
  overflow: hidden;
  width:702rpx ;
  height: 403rpx;
  z-index:3;
  border-radius:12rpx;
  margin-bottom: 28rpx;
  /* box-shadow: 5px 8px 27.84px 1.16px rgba(239, 60, 26, 0.26);
  text-shadow: 5px 8px 27.84px rgba(239, 60, 26, 0.26); */
}
.gropu_top_img{
  position: absolute;
  top: 0;
  left: 0;
  width:702rpx ;
  height: 403rpx;
  z-index: 1;
}
.group_bot{
  
}
.group_top_block{
  position: relative;
  z-index: 2;
  top: 0;
  padding: 29rpx 22rpx;
  background: none;
}
.group_titi{
  font-size: 22rpx;
  color: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.group_titi .group_titi_l text:nth-child(1){
  font-size: 30rpx;
}
.group_titi .group_titi_l text:nth-child(2){
  font-size: 24rpx;
  color: #FFCFD1;
  padding-left: 15rpx;
}
.group_titi .group_titi_r icon{
  display: inline-flex;
  font-size: 25rpx;
}
.group_list_block{
  padding-top: 22rpx;
  display: flex;
  justify-content: space-between;
}
.gropu_list_pro{
  width: 202rpx;
  color: #fff;
}
.gropu_list_pro .group_pro_img{
  width: 202rpx;
  height: 202rpx;
}
.gropu_list_pro .grop_list_num{
   display: flex;
   justify-content: center;
   align-items: center;
   padding:12rpx 0 8rpx;
}
.gropu_list_pro .grop_list_num text:nth-child(1){
  height: 28rpx;
  line-height: 28rpx;
  display: inline-block;
  border-radius: 18rpx;
  padding: 0 12rpx;
  font-size:20rpx;
}
.gropu_list_pro .grop_list_num text:nth-child(2){
  display: inline-block;
  font-size: 30rpx;
  padding-left: 6rpx;
}
.gropu_list_pro .group_al_num{
  text-align: center;
  font-size: 22rpx;
  color:#FFB8BB
}
.group_bot{
  padding: 0 24rpx;
}
.group_bot_block{
  /* margin-bottom:24rpx ; */
}
.group_bot_block_list{
  display: flex;
  background: #fff;
  padding: 20rpx;
  border-radius: 12rpx;
  margin-bottom:24rpx ;
  align-items: center;
}
.group_bot_l{
  position: relative;
}
.group_bot_l .pro_icon{
  position: absolute;
  width: 80rpx;
  height: 80rpx;
}
.group_bot_l .pro_icon_left_top{
  top: 0;
  left: 0;
}
.group_bot_l .pro_icon_right_top{
  top: 0;
  left: 0;
}
.group_bot_l .pro_icon_left_bottom{
  top: 0;
  left: 0;
}
.group_bot_l .pro_icon_right_bottom{
  top: 0;
  left: 0;
}
.group_bot_l .pro_main_image {
  height: 256rpx;
  width: 256rpx;
}
.group_bot_l .pro_min_frame{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.group_bot_r{
  width: 384rpx;
  padding-left: 18rpx;
}
.group_bot_r .group_bot_r_titi{
  color: #333;
  font-size: 26rpx;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  word-break: break-all;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.group_lable{
  margin-top: 15rpx;
}
.group_bot_r .group_bot_r_titi .label{
  color: #fff;
  font-size: 20rpx;
  display: inline-flex;
  padding: 0 8rpx;
  height: 28rpx;
  border-radius: 14rpx;
  align-items: center;
  margin-right: 10rpx;
}

.group_bot_r .group_bot_r_dis{
  height: 30rpx;
  border-radius: 4rpx;
  padding:0 6rpx;
  font-size: 20rpx;
  margin-right: 10rpx;
  display: inline-block;
}
.group_bot_r .group_bot_r_dis:last-child{
  margin-right: 0;
}
.grop_bot_r_team{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-top: 40rpx;
}
.grop_bot_r_team .grop_bot_r_team_l text:nth-child(1){
  font-size: 30rpx;
  font-weight: 600;
}
.grop_bot_r_team .grop_bot_r_team_l text:nth-child(2){
   display: inline-block;
   font-size: 22rpx;
   color: #C1C1C1;
   padding-left: 10rpx;
   text-decoration: line-through;
}
.grop_bot_r_team_r{
  color: #fff;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 30rpx;
  padding: 0 25rpx;
  font-size: 26rpx;
}