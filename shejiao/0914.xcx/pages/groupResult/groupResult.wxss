
page{
  background: #f9f9f9;
}
.result_group{
}
.result_group .group_order{
  padding:20rpx 0;
}
.result_group .group_order .group_pro{
  background: #fff;
  border:2rpx solid #e0e0e0;
  border-radius: 16rpx;
  display: flex;
  align-items: center;
  padding: 26rpx 22rpx;
  margin: 0 20rpx;
}
.result_group .group_order .group_pro .pro_img{
  width: 180rpx;
  height: 180rpx;
  margin-right: 20rpx;
}
.result_group .group_order .group_pro .pro_img image{
  width: 100%;
  height: 100%;
  border-radius: 4rpx;
}
.result_group .group_order .group_pro .pro_info{
  font-size: 28rpx;
  width: 460rpx;
}
.result_group .group_order .group_pro .pro_info .title{
  width: 460rpx;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
}
.result_group .group_order .group_pro .pro_info .group_num{
  display: flex;
  align-items: center;
  font-size: 24rpx;
  margin: 6rpx 0;
}
.result_group .group_order .group_pro .pro_info .group_num .group_tips{
  background: #e1381620;
  color: #e13816;
  min-width: 60rpx;
  padding: 2rpx 8rpx;
}
.result_group .group_order .group_pro .pro_info .group_num .group_tips .iconfont{
  display: inline-flex;
  align-items: center;
  font-size: 30rpx;
}
.result_group .group_order .group_pro .pro_info .price{
  font-size: 24rpx;
  color: #e13816;
}
.result_group .group_order .group_pro .pro_info .price text{
  font-size: 30rpx;
  font-weight: bold;
}
.result_group .group_order .group_pro .pro_info .price .market{
  color: #999;
  font-size: 20rpx;
}
.result_group .group_order .group_pro .pro_info .price .market text{
  font-size: 20rpx;
  font-weight: 500;
}
/* 团购信息，倒计时，成员等 */
.result_group .group_order .group_info{
  padding: 30rpx 20rpx;
  font-size: 24rpx;
  margin: 0 20rpx;
}
.result_group .group_order .group_info .group_countdown{
  text-align: center;
}
.result_group .group_order .group_info .group_countdown .num{
  color: #e13816;
  margin: 0 4rpx;
}
.result_group .group_order .group_info .group_countdown .time{
  color: #fff;
  background: #e13816;
  padding: 2rpx 4rpx;
  border-radius: 4rpx;
  margin: 0 4rpx;
}
.result_group .group_order .group_info .group_person{
  width:600rpx;
  white-space: nowrap;
  margin: 0 auto;
  text-align: center;
  margin-top: 20rpx;
}
.result_group .group_order .group_info .group_person .group_img{
  margin: 0 14rpx;
  position: relative;
  color: #e13816;
  display: inline-block;
}
.result_group .group_order .group_info .group_person .group_img:first-child{
  margin: 0 0;
}
.result_group .group_order .group_info .group_person .group_img .leader {
  position: absolute;
  /* top: 12rpx; */
  background: #e13816;
  color: #fff;
  /* height: 30rpx; */
  /* width: 64rpx; */
  font-size: 20rpx;
  text-align: center;
  border-radius: 6rpx;
  padding: 4rpx 10rpx;
  left: 50%;
  transform: translateX(-50%);
  top: 0;
}
.result_group .group_order .group_info .group_person .group_img .img_bg {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background: #f7f7f7;
  border: 2rpx solid #e0e0e0;
}

.result_group .group_order .group_info .group_person .group_img image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  border: 2rpx solid;
  /* border: 2rpx solid #0d8d37; */
}
/* 拼团按钮 */
.result_group .group_order .group_btn{
  display: flex;
  align-items: center;
  width: 80%;
  margin: 0 auto;
  justify-content: space-between;
}
.result_group .group_order .group_btn button{
  color: #fff;
  font-weight: normal;
  font-size: 32rpx;
  padding: 14rpx 0;

}
.result_group .group_order .group_btn .btn_normal{
  width: 100%;
}
.result_group .group_order .group_btn .btn_short{
  width: 48%;
}
.result_group .group_order .group_btn .success{
  background: #1AAD19;
}
.result_group .group_order .group_btn .fail{
  background: #e0e0e0;
  color: #999;
}
.result_group .group_order .group_btn .share{
  background: linear-gradient(263deg,rgba(221,44,47,1) 0%,rgba(255,80,83,1) 100%);
}
.result_group .group_order .group_btn .join{
  background: linear-gradient(263deg,rgba(221,44,47,1) 0%,rgba(255,80,83,1) 100%);
}
.result_group .group_order .group_tips_text{
  color: #e13816;
  font-size: 28rpx;
  margin: 20rpx auto;
  text-align: center;
}
