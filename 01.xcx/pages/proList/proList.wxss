}

.pro_info .pro_title  image {
  width: 68rpx;
  height: 24rpx;
}

.pro_title  .pro_level {
  width: 68rpx;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  position: relative;
  height: 24rpx;
}

.pro_title  .pro_level text {
  font-size: 16rpx;
  color: #fefefe;
  position: absolute;
  left: 46%;
  top: 50%;
  transform: translate(-50%, -50%);
  white-space: nowrap;
}

.pro_level .iconfont {
  color: #ff8d97;
  display: inline-flex;
}

.pro_level text {
  color: #fff;
  font-size: 22rpx;
}

.pro_info .pro_spec {
  /* padding: 0 26rpx; */
  text-align: left;
  /* padding-bottom: 10rpx; */
  margin-top: 10rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.pro_spec .pro_price {
  font-family: 'DIN';
  width: 100%;
  /* display: flex;
  align-items: center; */
}

.pro_price .peo_nowprice {
  color: #ff8d97;
  font-size: 32rpx;
  font-weight: bold;
}

.pro_price .pro_integral {
  font-size: 32rpx;
  font-weight: bold;
  color: #ff8d97;
  /* font-weight: bold; *//* margin-left: 8rpx; */
}

.pro_price .pro_inter_img {
  display: inline-flex;
  align-items: flex-end;
  /* margin-left: 9rpx; *//* margin-right: 30rpx; */
}

.pro_price .pro_inter_img .iconfont {
  font-size: 32rpx;
  font-weight: bold;
  color: #ff8d97;
  display: inline-flex;
}

.pro_price .pro_orig {
  font-size: 24rpx;
  color: #888;
}

.pro_price .pro_bg {
  background: #ff8d97;
  color: #fff;
  border-radius: 8rpx;
  text-align: center;
  font-size: 20rpx;
  margin-right: 10rpx;
  /* width: 100rpx; */
  padding: 0 8rpx;
}

.pro_price .line {
  text-decoration: line-through;
  /* margin-left: 14rpx; */
}

.pro_price .pro_count {
  font-size: 23rpx;
  color: #888;
}

.pro_spec .pro_btn {
  margin-right: 4rpx;
  display: flex;
  border-radius: 6rpx;
}

.pro_other {
  display: flex;
  justify-content: space-between;
  font-size: 23rpx;
  color: #999;
  padding: 0 26rpx;
  padding-bottom: 10rpx;
}

.pro_btn button {
  width: 170rpx;
  height: 52rpx;
  background: #ff8d97;
  color: #fff;
  font-size: 26rpx;
  padding: 0rpx 20rpx;
  line-height: 52rpx;
  border-radius: 50rpx;
}

.pro_btn button::after {
  border: none;
}

.order-red {
  height: 84rpx;
  line-height: 84rpx;
  /* background: #fff; */
  text-align: center;
  background: #f2f2f2;
}

.order-red text {
  font-size: 23.97rpx;
  color: #bebebe;
}

/* 空白的块，解决底部导航占领高度 */

.null_height {
  height: 100rpx;
}

checkbox .wx-checkbox-input {
  border-radius: 50%;
  width: 34rpx;
  height: 34rpx;
  border: none;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border: 1px solid transparent;
  background: #fff;
  color: #ff8d97;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  border-radius: 50%;
  width: 34rpx;
  height: 34rpx;
  line-height: 34rpx;
  text-align: center;
  font-size: 30rpx;
  color: #ff8d97;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}
