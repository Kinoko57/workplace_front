page{
  background: #f6f6f6;
}
.after_detail_pro{
  background: #fff;
  color: #666;
  font-size: 18rpx;
  text-align: center;
  padding: 20rpx 0 10rpx;
}
.after_detail_top{
  background: #e13816;
  color: #fff;
  padding: 0 40rpx 60rpx;
}
.after_detail_status{
  padding: 30rpx 0 20rpx;
  font-size: 34rpx;
}
/* 进度 */
.after_datail_progress{

}

.vip_sign_date_wraper{
  display: flex;
  flex-wrap: wrap;
}
.vip_sign_block .vip_sign_t{
   display: flex;
   align-items: center;
}
.vip_sign_block .vip_sign_t icon{
  display: inline-flex;
  align-items: center;
  font-size: 24rpx;
}
.progress_line{
  width: 100rpx;
  height: 4rpx;
  background: #fff;
}
.vip_sign_block:last-child .progress_line{
  width: 0;
}
.vip_sign_block .vip_sign_b{
   font-size: 20rpx;
   color: #fff;
   padding-top: 10rpx;
   width: 100rpx;
}
.after_detail_bot{
  padding: 0 20rpx;
  position: relative;
  top: -30rpx;
}
.after_detail_block{
  background: #fff;
  border-radius: 20rpx;
  padding:30rpx;
  margin-bottom:30rpx ;
}
.after_detail_block_other{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.server_msg{
  font-size: 22rpx;
  color: #333;
  width: 620rpx;
  word-break: break-all;
}
.server_msg text:nth-child(2){
  font-size: 22rpx;
  color: #999;
  display: block;
  padding-top: 15rpx;
}
.pro_block{
  padding-bottom: 20rpx;
}
.pro_block .pro_block_titi{
  font-size: 30rpx;
  color: #333;
  padding-bottom: 20rpx;
}
.pro_block .pro_block_list{
   display: flex;
   padding: 15rpx 0;
   border-bottom: 1px solid #f6f6f6;
}
.pro_block .pro_block_list:last-child{
  border-bottom: 0;
}
.pro_block_list .pro_block_list_img{
  width: 134rpx;
  height: 134rpx;
}
.pro_block_list .pro_block_list_info{
   width: 520rpx;
   padding-left: 20rpx;
   color: #999;
   font-size: 20rpx;
   padding-top: 10rpx;
}
.pro_block_list_info .pro_block_list_titi{
  line-height: 1.4;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  word-break: break-all;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  width: 460rpx;
}
.pro_block_list_info .pro_block_list_pri{
  display: flex;
  padding-top: 14rpx;
}
.pro_block_list_pri .list_pri:nth-child(2){
   padding-left: 20rpx;
}
.pro_block_list_pri .list_pri .pri{
  color: #333;
  font-size:24rpx ;
}
.server_order{
  font-size: 24rpx;
  border-bottom: 1px solid #ededed;
  padding-bottom: 25rpx;
  padding-top: 15rpx;
}
.server_order:last-child{
  border-bottom: 0;
}
.server_order:first-child{
  padding-top: 0;
}
.server_order_block{
  display: flex;
  padding: 20rpx 0;
}
.server_order_block_l{
   color: #999;
}
.server_order_block_r{
   color: #333;
   width: 510rpx;
   padding-left: 6rpx;
   word-break: break-all;
}
.server_order_block_copy{
  display: inline-block;
  height:32rpx;
  line-height: 32rpx;
  font-size: 18rpx;
  width: 70rpx;
  text-align: center;
  border:1px solid #999;
  border-radius: 16rpx;
  margin-left: 10rpx;
}
.server_delet{
  color: #999;
  border-radius: 18rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 22rpx;
  border:1px solid #999;
  width: 160rpx;
  text-align: center;
  margin: 0 auto 30rpx;
}
.server_fixed{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 710rpx;
  background: #fff;
  text-align: right;
  padding: 20rpx;
}
.server_fixed text{
  display: inline-block;
  height: 60rpx;
  padding: 0 20rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  text-align: center;
  border-radius: 30rpx;
  margin-left: 10rpx;
}
.server_fixed text.cancel{
  border: 1px solid #666;
}
.server_fixed text.active{
  border: 1px solid #e13816;
}

