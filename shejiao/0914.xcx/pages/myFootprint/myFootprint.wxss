/* pages/myFootprint/myFootprint.wxss */
page{
  background: #f6f6f6;
}
.my_footprint {
  /* font-family: SimHei; *//* background: #f2f2f2; */
  overflow-x: hidden;
}

.foot_header {
  text-align: center;
  background: #f6f6f6;
  width: 100%;
  height: 86rpx;
  line-height: 86rpx;
  display: flex;
  justify-content: space-between;
  position: fixed;
  top: 0;
  z-index: 100;
}

.foot_header text {
  padding-left: 26rpx;
  color: #515151;
  font-size: 24rpx;
}

.foot_header .foot_num {
  color: #e33b16;
  font-size: 26rpx;
  padding-left: 0;
}

.foot_header .foot_edit {
  float: right;
  padding-right: 26rpx;
  color: #e33b16;
  font-size: 24rpx;
}

.foot_body {
  height: 100%;
  /* padding-top: 86rpx; */
}

.top {
  padding-top: 0;
}

.foot_body  .foot_date {
  padding: 0 10rpx;
  /* background: #f8f8f8; */
  background: #fff;
  position: relative;
  /* overflow: hidden; */
  margin-bottom: 30rpx;
  border-bottom: 2rpx solid #efefef;
  /* height: 414rpx; 原始高度 */
}

/* .foot_body  .foot_date_hidden {
  padding: 0 10rpx;
  background: #f8f8f8;
  position: relative;
  overflow: hidden;
  height: 170rpx;
} */
.foot_date .down_open{
  width: 60rpx;
  height: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 50%;
  background: #fff;
  box-shadow: 2rpx 2rpx 4rpx 0px rgba(9, 2, 4, 0.15);
  position: absolute;
  right: 20rpx;
  bottom: -30rpx;
}
.foot_date .down_more  icon {
transform: rotate(90deg);

}

.foot_date .down_more_down icon {
transform: rotate(270deg);
}

.foot_date icon {
  color: #cecece;
  display: inline-flex;
  align-items: center;
  background: #fff;
  font-size: 36rpx;
}

.foot_list {
  /* padding-top: 32rpx; */
}

.foot_list .foot_pro_block .chose_date {
  font-size: 28rpx;
  margin-left: 26rpx;
  font-family: 'DIN';
  padding: 10rpx 0;
}
.foot_list .foot_pro_block .show{
  color: #686868;
}
.foot_list .foot_pro_block .hidden{
  color: transparent;
  display: none;
}

.foot_list .foot_pro_block{
margin-bottom: 20rpx;
}
.foot_list .clear{
  overflow: hidden;
  clear: both;
}
.foot_list .hide_right {
  margin-right: 0;
}
.foot_list .coll_pro {
  height: 296rpx;
  background: #fff;
  margin: 20rpx;
  margin-bottom: 0;
  display: flex;
  align-items: center;
  flex-direction: row;
  overflow: hidden;
  border-radius: 12rpx;
}
.foot_list .coll_pro:first-child{
  margin-top: 0;
}
.foot_list .coll_pro movable-area {
  width: 750rpx;
  height: 296rpx;
}

.foot_list .coll_pro movable-area movable-view {
  width: 750rpx;
  height: 296rpx;
}

.foot_list .coll_pro movable-area movable-view .main_item_content {
  box-sizing: border-box;
  background: #ffff;
  width: 750rpx;
  height: 296rpx;
}

.foot_list .coll_pro .pro_block {
  display: inline-flex;
  align-items: center;
  height: 296rpx;
  /* margin: 0 26rpx; */
  background: #fff;
  width: 700rpx;
}

.foot_list .coll_pro .pro_block_short {
  width: 638rpx;
}

.coll_pro .pro_chose {
  float: left;
  display: flex;
  align-items: center;
  height: 296rpx;
  margin-left: 12rpx;
}

.coll_pro .pro_img {
  float: left;
  /* margin-top: 40rpx; */
  margin-left: 18rpx;
  margin-right: 30rpx;
  position: relative;
}

.pro_img image {
  width: 220rpx;
  height: 220rpx;
}

.pro_img .pro_tips {
  width: 220rpx;
  height: 52rpx;
  background: rgba(0, 0, 0, 0.4);
  line-height: 52rpx;
  text-align: center;
  position: absolute;
  bottom: 0;
}

.pro_tips text {
  color: #fff;
  font-size: 24rpx;
}

.coll_pro .pro_info {
  float: left;
  width: 420rpx;
}

.coll_pro .pro_info_edit {
  width: 360rpx;
}

.pro_info .pro_title {
  /* width: 440rpx; */
  font-size: 26rpx;
  color: #333;
  /* margin-top: 50rpx; */
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  line-height: 1.6;
}
.pro_info .pro_desc{
  margin: 0 auto;
  font-size:24rpx;
  color:rgba(153,153,153,1);
  overflow: hidden;
  /*超出部分隐藏*/
  white-space: nowrap;
  /*不换行*/
  text-overflow: ellipsis;
  margin-bottom: 10rpx;
}
.pro_title image {
  width: 68rpx;
  height: 24rpx;
}

.pro_title  .pro_icon {
  background: #e13816;
  height: 26rpx;
  padding: 0 6rpx;
  font-size: 20rpx;
  color: #fff;
  border-radius: 8rpx;
  line-height: 26rpx;
  text-align: center;
  display: inline-block;
  margin-right: 10rpx;
}

.pro_title  .pro_level {
  width: 68rpx;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  position: relative;
  height: 24rpx;
}

.pro_title  .pro_level text {
  font-size: 16rpx;
  color: #fefefe;
  position: absolute;
  left: 46%;
  top: 50%;
  transform: translate(-50%, -50%);
  white-space: nowrap;
}

.pro_info .pro_spec {
  width: 100%;
  /* width: 440rpx; */
  overflow: hidden;
  margin-top: 20rpx;
  height: 64rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.pro_spec .pro_price {
  font-family: 'DIN';
  width: 280rpx;
  display: flex;
  align-items: center;
}

.pro_price .pro_nowprice {
  color: #e13816;
  font-size: 30rpx;
  font-weight: bold;
}

.pro_price .pro_integral {
  font-size: 30rpx;
  font-weight: bold;
  color: #e13816;
  /* font-weight: bold; */
}

.pro_price .pro_inter_img {
  display: flex;
  align-items: flex-end;
  /* margin-right: 6rpx; */
}

.pro_price .pro_inter_img .iconfont {
  font-size: 26rpx;
  display: inline-flex;
  color: #e13816;
  font-weight: bold;
}

.pro_price .pro_orig {
  font-size: 22rpx;
  color: #7c7c7c;
  text-decoration: line-through;
}

.pro_spec .pro_icon {
  margin-right: 4rpx;
  display: flex;
}

.pro_icon image {
  width: 50rpx;
  height: 46rpx;
}

.pro_icon button {
  width: 170rpx;
  height: 52rpx;
  background: #e13816;
  color: #fff;
  font-size: 26rpx;
  padding: 0rpx 20rpx;
  line-height: 52rpx;
  border-radius: 50rpx;
}

.foot_list .delete_btn {
  width: 184rpx;
  height: 296rpx;
  background-color: #e86d56;
  border-top-right-radius: 10px;
  border-bottom-right-radius: 10px;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
  line-height: 296rpx;
}

.order-foot {
  height: 84rpx;
  line-height: 84rpx;
  /* background: #fff; */
  text-align: center;
}

.order-foot text {
  font-size: 23.97rpx;
  color: #bebebe;
}

.foot_list_null {
  text-align: center;
  font-size: 30rpx;
  /* font-weight: bold; */
  padding:80rpx 0;
  color: #999;
}
.foot_list_null image{
  display: inline-block;
  width: 750rpx;
  height: 357rpx;
}

.foot_edit_more {
  box-shadow:0px 0px 24rpx 0px rgba(0, 0, 0, 0.19);
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: fixed;
  bottom: 0;
  width: 100%;
  background: #fff;
  z-index: 999;
}
.editDel_null {
  height: 100rpx;
}

.editDel_null_iphoneX {
  height: 168rpx;
}
.editDel_iphonex {
  padding-bottom: 68rpx;
}

.foot_edit_more .allCheck {
  margin-left: 26rpx;
  font-size: 26rpx;
  color: #555;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.foot_edit_more .del_btn {
  width: 240rpx;

}
.foot_edit_more .del_btn button {
  width: 156rpx;
  margin: 0 auto;
  padding: 0;
  border-radius: 0;
  color: #fff;
  font-size: 26rpx;
  font-weight: normal;
  height:60rpx;
  line-height:60rpx;
  border:1px solid rgba(255,0,0,1);
  color: rgba(255,0,0,1);
  border-radius: 50rpx;
}

.foot_edit_more .del_btn .disbale_btn {
  background: rgba(225, 56, 22, 0.5);
}

checkbox .wx-checkbox-input {
  border-radius: 50%;
  width: 34rpx;
  height: 34rpx;
  /* border: 1px solid #d8d8d8;
  background: #484848; */
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border: 1px solid transparent;
  background: #e33b16;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  border-radius: 50%;
  width: 34rpx;
  height: 34rpx;
  line-height: 34rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}
