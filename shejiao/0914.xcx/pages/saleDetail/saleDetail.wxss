/* pages/saleAfter/saleAfter.wxss */
page{
  background: #f6f6f6;
}
.saleAfter_body_top{
  background: #fff;
  padding: 0 27rpx;
}
.saleAfter_body_top_pro{
  padding-bottom: 28rpx;
}
.proSale_body_top_block{
  display: flex;
  align-items: center;
  padding: 30rpx 0 10rpx;
  border-bottom: 1px solid #f1f1f1;
}
.proSale_body_top_block:last-child{
  border-bottom: 0;
}
.saleAfter_body_top_pro_img{
  width: 130rpx;
  height: 130rpx;
}
.saleAfter_body_top_pro_r{
   padding-left: 20rpx;
}
.saleAfter_body_top_pro_r .saleAfter_body_top_pro_titi{
   color: #666;
   font-size: 24rpx;
   width: 530rpx;
   line-height: 1.4;
   display: -webkit-box;
   overflow: hidden;
   text-overflow: ellipsis;
   word-wrap: break-word;
   word-break: break-all;
   white-space: normal !important;
   -webkit-line-clamp: 2;
   -webkit-box-orient: vertical;
}
.saleAfter_body_top_pro_r .saleAfter_body_top_pro_info{
  color: #666;
  font-size: 24rpx;
  display: flex;
  padding-top: 6rpx;
}
.saleAfter_body_top_pro_info .proSale_info_pri{
  padding-right: 10rpx;
}
.saleAfter_info_pri{
  padding-right: 30rpx;
}
.saleAter_block{
  background: #fff;
  border-radius: 20rpx;
  margin-top: 20rpx;
  padding: 0 18rpx 26rpx;
}
.saleAfter_tit{
  display: flex;
  justify-content: space-between;
  padding: 21rpx 14rpx 24rpx;
  border-bottom: 1px solid #ededed;
}
.text_left{
  font-size: 24rpx;
  color: #666;
}
.text_right{
  font-size: 28rpx;
  color: #000;
}
.saleAfter_teaxtarea{
  width:690rpx;
  height: 120rpx;
  background: #f4f2f2;
  border-radius: 20rpx;
  margin-top: 18rpx;
  padding:15rpx;
}
.saleAfter_teaxtarea textarea{
  width:690rpx;
  height: 90rpx;
}
.saleAfter_teax_num{
  font-size: 24rpx;
  color: #666;
  text-align: right;
}
.saleAter_block .text_class{
  font-size: 24rpx;
  color: #b3b1b1;
}
.saleAfter_upload{
  padding-top: 22rpx;
}
.saleAfter_upload_block{
  display: flex;
  flex-wrap: wrap;
}
.saleAfter_upload_block .upload_block{
  position: relative;
  width: 154rpx;
  height: 154rpx;
  margin-right: 15rpx;
  margin-bottom: 10rpx;
}
.saleAfter_upload_block .saleAfter_upload_block_img{
  width: 154rpx;
  height: 154rpx;
  border-radius: 10rpx;
}
.saleAfter_upload_block .upload_block icon{
  position: absolute;
  top: 0;
  right: 0;
  font-size: 24rpx;
}
.saleAfter_upload_add{
  width: 152rpx;
  height: 152rpx;
  border: 1px solid #e5e5e5;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 10rpx;
}
.saleAfter_upload_add_text{
  text-align: center;
}
.saleAfter_upload_add_text text{
  display: block;
  font-size: 18rpx;
  color: #666;
}
.saleAfter_upload_add_text icon{
  display: inline-flex;
  align-items: center;
}
.saleAfter_upload_tips{
  font-size: 18rpx;
}
.saleAter_addre_info{
  padding-top: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.saleAter_addre_info_text{
  color: #020202;
  width: 660rpx;
  padding-left: 10rpx;
  font-size: 24rpx;
  line-height: 1.5;
  word-break: break-all;
}
.saleAter_block icon{
  color: #000;
  display: inline-flex;
}
.userInfo_block{
  display: flex;
  color: #000;
  font-size: 24rpx;
  padding-top: 26rpx;
  height: 80rpx;
  align-items: center;
  border-bottom: 1px solid #ededed;
}
.userInfo_block:last-child{
  border-bottom: 0;
  padding-top: 0;
}
.userInfo_block .text_user{
  color: #000;
  font-size: 24rpx;
}
.userInfo_block input{
  padding-left: 12rpx;
  width: 600rpx;
}
.saleAter_order_info{
  padding-top: 20rpx;
}
.after_tracking {
  background: #fff;
  padding-left: 26rpx;
  display: flex;
  align-items: center;
  height: 80rpx;
}
.after_tracking text {
  font-size: 26rpx;
}
.after_tracking input{
  width: 530rpx;
}
.saleAter_order_info_tracking{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.saleAter_order_info_tracking .picker_text{
  font-size: 24rpx;
  display: inline-flex;
  align-items: center;
  color: #666;
  width: 500rpx;
  justify-content: flex-end;
}
.saleAter_order_info_tracking .picker_text icon{
  display: inline-flex;
  font-size: 26rpx;
}
.sale_submit{
  width: 690rpx;
  margin: 70rpx auto;
  height: 78rpx;
  border-radius:39rpx;
  line-height:78rpx;
  color: #fff;
  text-align: center;
}