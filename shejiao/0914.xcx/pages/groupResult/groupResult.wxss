/* 购物车推荐 */
.car_recommend_list {}

.car_recommend_list .car_recommend_title {
  padding: 20rpx 10rpx;
  font-size: 30rpx;
  color: #333333;
  display: flex;
  align-items: center;
  justify-content: center;
}

.car_recommend_list .car_recommend_title .more {
  font-size: 26rpx;
  display: inline-flex;
  align-items: center;

}

.car_recommend_list .car_recommend_title .more text {
  display: inline-flex;
  align-items: center;
}

.car_recommend_list .car_recommend_title .more .more_link {
  display: inline-flex;
  align-items: center;
  font-size: 28rpx;
}

.car_recommend_list .car_pro_list {
  padding: 0 20rpx;
  overflow: hidden;
}

.car_recommend_list .car_pro_list .pro_list_block:nth-child(2n+2) {
  margin-right: 0;
}
/* 商品规格弹框 */

.spec_model {
  height: 80vh;
  top: calc(100% - 80vh);
  animation: specSheet 300ms ease-out;
}

.spec_model .model_body {
  padding: 20rpx;
  height: 70%;
}

.spec_model .model_body .spec_pro {
  display: flex;
  align-items: center;
  position: relative;
  border-bottom: 2rpx solid #e0e0e0;
  padding-bottom: 20rpx;
}

.spec_model .model_body .spec_pro .spec_pro_img {
  width: 200rpx;
  height: 200rpx;
  margin-right: 26rpx;
}

.spec_model .model_body .spec_pro .spec_pro_img image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.spec_model .model_body .spec_pro .spec_pro_price {
  font-size: 24rpx;
  color: #333;
}

.spec_model .model_body .spec_pro .spec_pro_price .price {
  font-size: 40rpx;
  color: #e86d56;
  margin-bottom: 10rpx;
}

.spec_model .model_body .spec_pro .spec_pro_price .store {
  color: #999;
  margin-bottom: 6rpx;
}

.spec_model .model_body .spec_pro .close_btn {
  position: absolute;
  right: 0rpx;
  top: 0;
  font-size: 42rpx;
  color: #666;
}

.spec_model .model_body .spec_list {
  padding: 20rpx 0;
  border-bottom: 2rpx solid #e0e0e0;
}

.spec_model .model_body .spec_list .spec_title {
  font-size: 28rpx;
  color: #333;
  margin-bottom: 20rpx;
  /* margin: 20rpx 0; */
}

.spec_model .model_body .spec_list .spec_block_list {
  overflow: hidden;
}

.spec_model .model_body .spec_list .spec_block_list .spec_block {
  /* height: 70rpx; */
  float: left;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
  /* min-width: 120rpx; */
  padding: 10rpx 16rpx;
  color: #333;
  font-size: 24rpx;
  background: #f5f5f5;
  border: 2rpx solid #f5f5f5;
  border-radius: 10rpx;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  border: 2rpx solid transparent;
}

.spec_model .model_body .spec_list .spec_block_list .spec_active {
  border: 2rpx solid #e86d56;
  background: #fff9f8;
  color: #e86d56;
}
.spec_model .model_body .spec_list .spec_block_list .spec_disable{
  border: 2rpx dashed #e0e0e0;
  color:#999;
}
.spec_model .model_body .spec_list .spec_block_list .spec_block image {
  height: 50rpx;
  width: 50rpx;
  margin-right: 4rpx;
}

.spec_model .model_body .spec_num {
  display: flex;
  align-items: center;
  padding: 20rpx 0;
  justify-content: space-between;
}
.spec_model .model_body .spec_num .num_input{
  display: inline-flex;
  align-items: center;
  justify-content: center;
}
.spec_model .model_body .spec_num .iconfont{
  font-size: 28rpx;
  display: inline-flex;
  align-items: center;
  margin: 0 8rpx;
}
.spec_model .model_body .spec_num input{
  background: #f5f5f5;
  border-radius: 6px;
  text-align: center;
  width: 110rpx;
  font-size: 26rpx;
  color: #333333;
  /* margin: 0 14rpx; */
}
.spec_model  .model_footer{
  display: flex;
  align-items: center;
  justify-content: center;
}
.spec_model  .model_footer .spec_btn{
  font-weight: normal;
  width: 320rpx;
  height: 80rpx;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  margin: 0;
  font-size: 30rpx;
  color: #ffffff;
  border-radius: 0;
}
.spec_model  .model_footer .add_car_btn{
  background: linear-gradient(90deg,rgba(70,70,70,1) 0%,rgba(47,47,47,1) 100%);
  border-top-left-radius: 50rpx;
  border-bottom-left-radius: 50rpx;
}
.spec_model  .model_footer .buy_now_btn{
  background: linear-gradient(263deg,rgba(221,44,47,1) 0%,rgba(255,80,83,1) 100%);
  /* border-top-right-radius: 50rpx;
  border-bottom-right-radius: 50rpx; */
  border-radius: 50rpx;
  width: 500rpx;
}
.spec_model  .model_footer .buy_disable_btn{
  /* background: linear-gradient(263deg,rgba(221,44,47,1) 0%,rgba(255,80,83,1) 100%); */
  background: #f5f5f5;
  border-radius: 50rpx;
  width: 640rpx;
  color: #999;
}
@keyframes specSheet {
  from {
    top: 100%;
  }
  to {
    top: calc(100% - 80vh);
  }
}
.activity_list_two_columns .pro_block_style:nth-child(even){
  float: right;
  margin-right: 0;
}