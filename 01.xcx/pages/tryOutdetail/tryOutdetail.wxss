/* pages/tryOutdetail/tryOutdetail.wxss */
/* 富文本解析组件 */
@import "/wxParse/wxParse.wxss";
page {
  background: #f2f2f2;
}

.try_detail {
  /* font-family: SimHei; */
  background: #f2f2f2;
  overflow-x: hidden;
}

.try_body {
  /* height: 100%; */
}

.try_pro_info {
  /* height: 880rpx; */
  background: #fff;
  margin-bottom: 10rpx;
}

.pro_img {
  /* position: relative; */
  text-align: center;
  /* padding-top: 54rpx; *//* padding-bottom: 47rpx; *//* height: 640rpx; *//* display: flex; *//* align-items: center; *//* justify-content: center; *//* height: 820rpx; */
}

.pro_img .pro_swiper {
  width: 100%;
  height: 750rpx;
}

.pro_swiper swiper {
  width: 100%;
  height: 100%;
  text-align: center;
}

.pro_swiper swiper-item {
  display: flex;
  align-items: center;
  justify-content: center;
}

swiper .slide-image {
  width: 750rpx;
  height: 750rpx;
}

.pro_img image {
  width: 750rpx;
  height: 750rpx;
}

.pro_down {
  /* position: absolute; */
  width: 100%;
  height: 72rpx;
  line-height: 72rpx;
  color: #fff;
  /* background: rgba(234, 53, 104, 0.3); */
  background-image: linear-gradient(267deg, #ab0023 0%, #e4002f 74%);
  font-size: 30rpx;
  /* font-weight: bold; */
  bottom: 0;
  display: flex;
  align-items: center;
  /* justify-content: center; */
  padding: 0 32rpx;
}

.pro_down text {
  font-size: 28rpx;
}

.pro_down .time_bg {
  color: #fff;
  background: #000;
  padding: 4rpx;
  /* border-radius: 6rpx; */
  font-family: 'DIN';
  margin: 0 4rpx;
  min-width: 45rpx;
  height: 45rpx;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
}

.pro_title {
  padding: 0 26rpx;
  margin: 50rpx 0;
  font-size: 32rpx;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.pro_other {
  padding: 0 26rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #686767;
  font-size: 34rpx;
  font-family: 'DIN';
  padding-bottom: 40rpx;
}

.pro_other .num {
  color: #ff8d97;
}

.pro_other .peo_num {
  color: #737373;
}

.pro_other .nowPrice {
  font-size: 30rpx;
  font-weight: bold;
  color: #ff8d97;
  margin-right: 10rpx;
}

.pro_other .price {
  font-size: 24rpx;
  text-decoration: line-through;
}

.try_pro_game {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 26rpx;
  /* margin-top: -10rpx; */
  margin-bottom: 10rpx;
  padding: 20rpx 26rpx;
  background: #fff;
}

.try_pro_title {
}

.try_pro_intrduce {
  display: inline-flex;
}

.try_pro_intrduce .iconfont {
  display: inline-flex;
  align-items: center;
  margin-left: 10rpx;
  margin-right: 4rpx;
  color: #808080;
  font-size: 26rpx;
}

.try_pro_intrduce text {
  color: #737373;
}

.try_pro_more {
  background: #fff;
}

.try_pro_more .pro_more_nav {
  display: flex;
  justify-content: center;
  font-size: 28rpx;
  color: #525151;
  padding: 34rpx 70rpx;
}

.pro_more_nav text {
  margin: 0 90rpx;
}

.pro_more_nav .nav_text_active {
  color: #ff8d97;
}

.peo_more_tab {
  /* padding: 0 26rpx; */
  text-align: center;
}

.peo_more_tab image {
  width: 700rpx;
  height: 100%;
}

.null_block {
  height: 100rpx;
  z-index: 180;
}

.null_view_height {
  height: 164rpx;
}

.try_btn {
  box-shadow: 0 8rpx 26rpx 14rpx #dadada;
  position: fixed;
  bottom: -6rpx;
  height: 100rpx;
  /* line-height: 100rpx; */
  display: flex;
  justify-content: space-between;
  /* align-items: center; */
  width: 100%;
  background: #fff;
  color: #fff;
  z-index: 199;
}

.iphone_height {
  height: 168rpx;
  background: #fff;
}

.try_btn button {
  display: flex;
  justify-content: center;
  /* width: 50%; */
  border-radius: 0;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
  color: #666;
}

.try_btn button::after {
  border: none;
}

.try_btn .pro_buy {
  /* width: 300rpx; *//* background: #ff8a00; */
  background: #000;
  color: #fff;
  width: 40%;
}

.try_btn  .application {
  width: 100%;
  background: #ff8d97;
  color: #fff;
}

.try_btn  .end_time {
  width: 100%;
  background: #d3d3d3;
  color: #fff;
}

.share_bg {
  position: fixed;
  z-index: 300;
  background: rgba(0, 0, 0, 0.4);
  width: 100%;
  height: 100%;
  top: 0;
}

.share_info {
  /* background: #e67354; */
  background: #ff8d97;
  width: 100%;
  height: 740rpx;
  position: fixed;
  bottom: 0;
  z-index: 301;
  text-align: center;
  padding: 2% 0;
}
.share_info_iphone{
  height: 808rpx;
}
.close {
  margin-right: 5%;
  text-align: right;
}

.close .iconfont {
  color: #fff;
  font-size: 40rpx;
}

.share_title {
  /* color: #ffe253; */
  color: #fff;
  font-size: 44rpx;
  font-weight: bold;
  margin-bottom: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.share_title text {
  color: #fff;
  font-weight: normal;
}

.share_title .over_hidden {
  width: 260rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: inline-block;
  margin-right: 10rpx;
}

.share_time {
  /* color: #ffe253; */
  color: #fff;
  margin-bottom: 40rpx;
}

.share_time .time {
  color: #fff;
  background: #000;
  /* background: #ffe253;  color: #6d4d23; */
  border-radius: 6rpx;
  font-family: 'DIN';
  margin: 0 4rpx;
  width: 50rpx;
  height: 50rpx;
  display: inline-flex;
  align-items: center;
  justify-content: center;
}

.share_people {
  background: #fff;
  border-radius: 10rpx;
  margin: 0 44rpx;
  padding: 30rpx 0;
}

.share_people .share_img_list {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 26rpx;
}

.share_img_list .share_img {
  width: 98rpx;
  height: 98rpx;
  border-radius: 50%;
  background: #e1e1e1;
  margin: 0 10rpx;
  border: 1px solid #e1e1e1;
}

.share_img_list .share_img image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.share_people .people_tips {
  color: #999;
  font-size: 30rpx;
  /* font-weight: bold; */
}

.share_people .people_tips .num {
  color: #ff8d97;
  font-family: 'DIN';
}

.share_info .share_btn {
  margin-top: 40rpx;
  display: flex;
}

.share_info .share_btn button {
  width: 44%;
  /* background: #ffe253;
  color: #6d4d23; */
  background: #fff;
  color: #ff8d97;
  height: 88rpx;
  line-height: 88rpx;
}

.share_info .share_btn .width {
  width: 60%;
}

.share_info .share_btn .two_width {
  width: 40%;
}

.share_info .share_btn .three_width {
  width: 30%;
}

.share_info .share_btn button::after {
  border: none;
}

/* 规格选择框 */

.spec_bg {
  position: fixed;
  background: rgba(0, 0, 0, 0.4);
  width: 100%;
  height: 100%;
  top: 0;
  z-index: 201;
}

.spec_info {
  width: 100%;
  background: #fff;
  position: fixed;
  bottom: 0;
  z-index: 202;
  min-height: 80vh;
}

.spec_head {
  height: 162rpx;
  padding: 0 26rpx;
}

.spec_pro_img {
  position: absolute;
  width: 170rpx;
  height: 170rpx;
  text-align: center;
  line-height: 170rpx;
  top: -35rpx;
  border: 2rpx solid #e0e0e0;
  border-radius: 10rpx;
  background: #fff;
}

.spec_pro_img image {
  width: 140rpx;
  /* height: 140rpx; */
  vertical-align: middle;
}

.spec_head .spec_pro_price {
  font-family: 'DIN';
  /* width: 280rpx; */
  display: flex;
  align-items: center;
  margin-left: 180rpx;
  margin-top: 24rpx;
}

.spec_pro_price .spec_pro_nowprice {
  color: #ff8d97;
  /* font-size: 60rpx; */
  font-size: 30rpx;
  font-weight: bold;
  /* margin-right: 16rpx; */
  border: 1px solid transparent;
  /* padding-right: 10rpx; */
}

.spec_pro_price .spec_pro_integral {
  font-size: 30rpx;
  font-weight: bold;
  color: #ff8d97;
  /* font-weight: bold; *//* margin-left: 8rpx; */
}

.spec_pro_price .spec_pro_inter_img {
  display: flex;
  align-items: flex-end;
  /* margin-left: 9rpx; *//* margin-right: 30rpx; */
}

.spec_pro_price .spec_pro_inter_img .iconfont {
  font-size: 30rpx;
  font-weight: bold;
  color: #ff8d97;
  display: inline-flex;
}

.spec_pro_price text {
  font-size: 24rpx;
  color: #b8b8b8;
  /* margin-left: 14rpx; */
}

.spec_pro_code {
  font-size: 26rpx;
  color: #9d9d9d;
  margin-left: 186rpx;
  margin-top: 10rpx;
  display: flex;
  justify-content: space-between;
}

.spec_pro_code .spec_pro_orig {
  font-size: 24rpx;
  color: #b8b8b8;
  text-decoration: line-through;
}

.spec_head .spec_close {
  position: absolute;
  top: 24rpx;
  right: 26rpx;
  display: inline-flex;
}

.spec_head .spec_close .iconfont {
  display: inline-flex;
}

.spec_body {
  padding: 0 26rpx;
  overflow: hidden;
  width: 700rpx;
  height: calc(80vh - 160px);
}

.spec_title {
  color: #9d9d9d;
  font-size: 24rpx;
  margin-bottom: 20rpx;
}

.spec_body .spec_type {
  margin-bottom: 30rpx;
}

.spec_type button {
  margin-right: 20rpx;
  margin-bottom: 10rpx;
  color: #555;
  font-size: 24rpx;
  background: #fff;
  border: 2rpx solid #777;
  line-height: 2.0;
  min-width: 116rpx;
}

.spec_type button .iconfont {
  font-size: 24rpx;
  display: inline-flex;
}

.spec_type button::after {
  border: none;
}

.spec_type .button_click {
  color: #ff8d97;
  border: 2rpx solid #ff8d97;
}

.spec_capacity {
  margin-bottom: 30rpx;
}

.spec_capacity button {
  margin-right: 20rpx;
  color: #555;
  font-size: 24rpx;
  background: #fff;
  border: 2rpx solid #777;
}

.spec_capacity button::after {
  border: none;
}

.spec_capacity .button_click {
  color: #ff8d97;
  border: 2rpx solid #ff8d97;
}

.spec_num {
  /* position: relative; */
  margin-bottom: 62rpx;
}

.spec_num text {
  color: #9d9d9d;
  font-size: 24rpx;
  margin-bottom: 20rpx;
}

.spec_num .pro_icon {
  margin-right: 4rpx;
  display: flex;
  border: 1px solid #ccc;
  border-radius: 6rpx;
  float: right;
  width: 160rpx;
}

.pro_icon button {
  width: 44rpx;
  height: 44rpx;
  line-height: 42rpx;
  padding: 0 22rpx;
  margin: 0;
  color: #9f9f9f;
  font-size: 38rpx;
  display: flex;
  justify-content: center;
  border-radius: 0;
  background: #f3f3f3;
}

.pro_icon  button::after {
  border: none;
}

.pro_icon input {
  width: 70rpx;
  height: 44rpx;
  min-height: 44rpx;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
  padding: 0;
  font-size: 30rpx;
  /* font-weight: bold; */
  color: #565656;
  text-align: center;
  display: flex;
  align-items: center;
  font-family: 'DIN';
}

.spec_btn {
  display: flex;
  align-items: center;
  position: absolute;
  width: 100%;
  bottom: 0;
}

.spec_btn_height {
  bottom: 68rpx;
}

.spec_btn .pro_buy {
  width: 50%;
  background: #ff8d97;
  color: #fff;
  font-size: 34rpx;
  border-radius: 0;
  height: 100rpx;
  line-height: 100rpx;
}

.spec_btn .add_car {
  width: 50%;
  background: #ff8a00;
  color: #fff;
  height: 100rpx;
  font-size: 34rpx;
  border-radius: 0;
  line-height: 100rpx;
}

.spec_btn .surebtn {
  width: 100%;
  background: #ff8d97;
  color: #fff;
  font-size: 34rpx;
  border-radius: 0;
}

.spec_btn button::after {
  border: none;
}
