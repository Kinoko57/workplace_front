}

.red_pro .pro_info {
  width: 100%;
}

.pro_info .pro_title {
  /* width: 320rpx; */
  font-size: 26rpx;
  color: #333;
  margin-top: 10rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  height: 70rpx;
