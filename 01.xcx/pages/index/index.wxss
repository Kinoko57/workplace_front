/* pages/index/index.wxss */
@import '../footer/footer.wxss';

.index {
  /* font-family: 'DIN'; */
  position: relative;
  background: #f2f2f2;
  overflow-x: hidden;
}

.i_header {
  padding: 0 30rpx;
  height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  /* position: absolute; */
  top: 0;
  width: 94%;
  z-index: 50;
  position: fixed;
  background: transparent;
}

.i_header_red {
  background: #ff8d97;
}

.i_header_bg {
  background: #fff;
}

.i_header .icon_bg {
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  background: rgba(0, 0, 0, 0.6);
  border-radius: 50%;
  position: relative;
}

.i_header .icon_bg .icon_radio {
  position: absolute;
  margin: 0;
  top: 4rpx;
  right: -2rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 26rpx;
  height: 26rpx;
  border-radius: 50%;
  background: #f00;
}

.i_header .icon_bg .icon_radio text {
  font-size: 14rpx;
  color: #fff;
  /* font-weight: bold; */
  display: inline-flex;
  align-items: center;
}

.i_header icon {
  color: #fff;
  font-size: 30rpx;
  font-weight: bold;
  display: flex;
  justify-content: center;
}

.i_header_red .icon_bg {
  background: transparent;
}

.i_header .search {
  width: 500rpx;
  height: 56rpx;
  line-height: 56rpx;
  border-radius: 50rpx;
  background: rgba(255, 255, 255, 0.7);
  padding: 0 30rpx;
}

.i_header .search_bg {
  background: #f2f2f2;
}

.i_header_red .search {
  border-top-right-radius: 0;
}

.i_header .search icon {
  color: #9b9b9b;
  font-size: 30rpx;
  font-weight: bold;
  float: left;
  display: flex;
}

.search text {
  font-size: 24rpx;
  color: #9b9b9b;
  float: left;
  width: 70%;
}

.i_model_bg {
  position: fixed;
  top: 92rpx;
  z-index: 110;
  background: rgba(0, 0, 0, 0.2);
  width: 100%;
  height: 100%;
}

